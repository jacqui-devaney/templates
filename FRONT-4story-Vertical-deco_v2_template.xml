			{if: =landscape, then: {small_headline:true, bottom:true, no_indent:true, no_border: {if: =scale.small, then:true}}}		
		]}},
		#b3: {options: {$switch: [
			{if: =scale.phone, then: {small_headline:true}},
			{if: =portrait, then: {small_headline:true, bottom:{if: =scale.small, then:true}, no_indent:true}},
			{if: =landscape, then: {small_headline:true, no_rule: {if: =scale.small, then:true}, no_border:true, no_indent:true}}		
		]}},
		#b4: {options: {small_headline:{if: =landscape, then:false, else:inherit}, no_image:true, no_indent:true}},
		#deco: {options: {$switch: [
			{if: =scale.phone, then: {normal_headline:true, no_border:true, G_image:true}},
