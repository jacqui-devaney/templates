{
	$join: [
	{ .line_black: { background-color:#000 }},
	{ .ad_dark: {background-color: #e4e4e4}},
	{ .ad_light: {background-color: #f6f5f3}},
	{ jpml: {
		font-size: 12,
		line-height: 15,
		image-scale: { if: =intent.template, then: {$max:[=scale.pagex,=scale.pagey]}, else: =scale.dpi },
		text-scale: 1,
		pixels-per-point-x: { if: =intent.template, then: =scale.pagex, else: =scale.dpi },
		pixels-per-point-y: { if: =intent.template, then: =scale.pagey, else: =scale.dpi }
	}},
	{ #highlight_ad: 	{ 
			anchor-x:center,
			anchor-y:middle,
			width:210,
			height:175
	}},
	{ if: =portrait, then: {
		#h1: 	{ frame: [1, 620, 256, 257] },
		#h2: 	{ frame: [257, 620, 256, 257] },
		#marketdata_background: 	{ 
			frame: [0, 620, 256, 257],
			border-width: [ 0, 1, 0, 0],
			border-color:#b1aca1
		},
		#highlight_marketdata: 	{ 
				anchor-x:center,
				anchor-y:middle,
				width:239,
				height:239
		},
		#ad_background: { frame: [513, 620, 255, 256]},
		#ad_background_PV: { frame: [510, 620, 258, 256]},
		#h3: 	{ frame: [0, 620, 513, 256] },
		#h4: 	{ frame: [0, 620, 513, 256] },
		#h5: 	{ frame: [0, 620, 255, 256] },
		#h6: 	{ frame: [255, 620, 255, 256] }
		
		
		
	}, else: {
		#h1: 	{ frame: [786, 0, 238, 216] },
		#h2: 	{ frame: [786, 216, 238, 207] },
		#marketdata_background: 	{ 
			frame: [786, 0, 238, 215], 
			border-width: [0, 0, 1, 0],
			border-color:#b1aca1
		},
		#highlight_marketdata: 	{ 
				anchor-x:center,
				anchor-y:middle,
				width:222,
				height:199
		},
		#ad_background: { frame: [786, 423, 238, 200] },
		#h3: 	{ frame: [786, 0, 238, 423] },
		#h4: 	{ frame: [768, 0, 256, 423] },
		#ad_background_PV: { frame: [768, 423, 256, 200] },
		#h5: 	{ frame: [768, 0, 256, 216] },
		#h6: 	{ frame: [768, 216, 256, 207] }
		
		
		
		
	
	}}
	]
}
