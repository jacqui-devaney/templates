<jpml id="FRONT-5story-deco_template">
	<style src="GLOBAL-stylesheet2.ejs" />
	<style src="GRID-stylesheet2.ejs"/>
	<style src="GRID-extras2.ejs"/>
	<style src="ADVERTISING-stylesheet.ejs"/>
	<style src="ADVERTISING-stylesheet-gridoverride.ejs"/>
	<style src="FRONT-stylesheet2.ejs"/>
	<style src="PHONE-stylesheet.ejs"/>
	<style src="RAIL-stylesheet.ejs" />
	<style>{
		#page_details: {
			options: {
				hideHeaderBorder: true
			}
		}
	}</style>
	<style> {
		$switch: [
			{if: =portrait, then: {
				#b1: {
					options:{ large_headline:true}
				},
				#deco: {
					options:{ medium_headline:true}
				},
				#b2: {
					options:{ medium_headline:true, top_border:true}
				},
				#b3: {
					options:{ medium_headline:true, top_border:true}
				},
				#b4: {
					options:{ medium_headline:true, top_border:true}
				},
				#b5: {
					options:{ medium_headline:true, bottom_align:true, show_byline:false, top_border:true}
				},
				#footer: {
					top: 930,
					height: 100,
					text-align: center,
					options:{top_border: true}
				}
			}},
			{if: =landscape, then: {
				#b1:	{
					options:{ large_headline:true}
				},
				#deco:	{
					options:{ medium_headline:true}
				},
				#b2:	{
					options:{ medium_headline:true, top_border:true}
				},
				#b3:	{
					options:{ medium_headline:true, top_border:true}
				},
				#b4:	{
					options:{ medium_headline:true, top_border:true}
				},
				#b5:	{
					options:{ medium_headline:true, top_border:true, show_byline:false}
				},
				#footer: {
					top: 680,
					height: 90,
					text-align: center,
					options:{top_border: true}
				}
		}}
		]
	}</style>
	<style>{
		.latest_news_box: {
			options: {
				reference: "//NOW/LATEST_NEWS"
			}
		}
	}</style>
	<box id="page_details" class="tablet_only" embed="pagedescription" />
	<box id="vertical-grid">
		<box class="tablet_only columns-3 xposition-1 yposition-3">
			<box class="columns-3">
				<box id="whatsnewsheader" class="visual-content-top">
					<p>
						<mark id="whatsnewsheadertext"/>
					</p>
				</box>
				<box id="whatsnews" embed="list" class="landscape-lines-24 portrait-lines-36" />
			</box>
		</box>
		<box id="deco" embed="article" class="lines-17 yposition-3 columns-9 xposition-4 maude unit_deco" module="DECO_module2.xml"/>
		<box id="b1" embed="article" class="portrait-yposition-20 portrait-lines-8 landscape-yposition-2 landscape-lines-18 landscape-columns-4 portrait-columns-9 landscape-xposition-13 portrait-xposition-4 maude unit_plain" module="TOP-IMAGE_module2.xml" />
		<box id="b2" embed="article" class="portrait-yposition-29 portrait-lines-7 landscape-yposition-21 landscape-lines-9 columns-3 xposition-4 maude unit_plain" module="TOP-IMAGE_module2.xml" />
		<box id="b3" embed="article" class="portrait-yposition-29 portrait-lines-7 landscape-yposition-21 landscape-lines-9 columns-3 xposition-7 maude unit_plain" module="TOP-IMAGE_module2.xml" />
		<box id="b4" embed="article" class="portrait-yposition-29 portrait-lines-7 landscape-yposition-21 landscape-lines-9 columns-3 xposition-10 maude unit_plain" module="TOP-IMAGE_module2.xml" />
		<box id="b5" embed="article" class="portrait-yposition-37 portrait-lines-5 landscape-yposition-21 landscape-lines-9 landscape-columns-4 portrait-columns-9 landscape-xposition-13 portrait-xposition-4 maude unit_plain" module="AHED_module2.xml" />
	</box>
	<box id="strap">
		<box class="tablet_only latest_news_box xposition-1">
			<p class="latest_news">LATEST NEWS</p>
		</box>
		<box id="marketstrap" class="tablet_only" embed="marketstrap">
			<box id="marketstrapinner">
				<panel id="marketstrappanel">
					<p id="marketstrapp1"><strong class="marketstrapstrong">DJIA</strong> 31337.42 +89.01 <mark class="marketstraparrow"/> 2.34%</p>
					<p id="marketstrapp2"><strong class="marketstrapstrong">NASDAQ</strong> 5678.90 +12.34 <mark class="marketstraparrow"/> 5.67%</p>
					<p id="marketstrapp3"><strong class="marketstrapstrong">S&amp;P 500</strong> 1234.56 +7.89 <mark class="marketstraparrow"/> 0.12%</p>
				</panel>
			</box>
		</box>
	</box>
	<box id="front_page" class="tablet_only positionx_full column_full maude unit_ad">
		<box id="banner_ad" class="ad-banner front_ad" embed="advertisement" />
	</box>
	<box id="footer" class="tablet_only landscape-columns-16 portrait-columns-12 xposition-1" module="RAIL-footer_module.xml">
		<box id="footerstrapinner">
			<p id="footer_headline" class="footer_headline">Did You Know?</p>
			<p id="footer_tip"><mark id="footer_tip" /></p>
		</box>
	</box>
</jpml>
