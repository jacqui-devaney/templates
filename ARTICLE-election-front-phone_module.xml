<jpml id="ARTICLE-election-front-phone_module">
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="ARTICLE-stylesheet.ejs" />
	<style src="PHONE-MODULES-stylesheet.ejs" />
	<style src="MEDIA-stylesheet.ejs" />
	<style>{
		jpml: {
			background-color: =options.background_color
		}
	}</style>
	<style>{
		jpml: { min-height: 0 },
		#body_container: {
			flow: article_body
		},
		.article_body: {
            hyphens: auto,
            padding: [0, 16, 0, 16]
		},
		.article_flashline: {
            font-name: WhitneySSm-Semibold,
            font-size: 13,
            line-height: 22,
            border-width: [0, 0, 1, 0],
            border-color: #e1e0df
        },
        #media_container:{
            padding: [0, 0, 19, 0]
        },
        #media_asset:{
            padding: [{if: =media.height, more-than: =media.width, then: -18, else: 0}, {if: =media.height, more-than: =media.width, then: 16, else: 4}, {if: =media.height, more-than: =media.width, then: -10, else: 10}, {if: =media.height, more-than: =media.width, then: 16, else: 4}]
        },
        #media_caption: {
            layout-grid-line: 1,
            layout-grid-line-offset: 0,
            color: #4f5757,
            font-size: 17,
            line-height: 22,
            font-name: "WhitneySSm-Book",
            padding: { if-all: [
                    { if: =page.number, equals: 0, then: true, else: false },
                    { if: =scale.phone, then: true, else: false }
                ], then: [0, 16, 0, 16], else: [0, 0, 0, 0]
        }},
        #mark_caption: {
            content: =media.caption
        }
	}</style>

    <style>{
        #headline_container:{
            background-color: #3b4343
        },
        #election: {
            padding: [4, 4, 0, 4]
        },
        #election_container:{
            padding: [12, 12, 12, 12],
            text-align: center,
            background-color: #dfe7df,
            display: {if: =app.push.tags.topic.politics, then: none, else: {if: =app.version, less-than: "7.4", then: none, else: inherit}}
        },
        .banner_headline: {
            padding: [8, {if: =view.width, equals: 375, then: 90, else: {if: =view.width, equals: 414, then: 99, else: 44}}, 10, {if: =view.width, equals: 375, then: 90, else: {if: =view.width, equals: 414, then: 99, else: 44}}],
            font-name: "WhitneySSm-Semibold",
            color: #fff,
            line-height: 22,
            font-size: 12,
            text-align: center,
            position: relative,
            width: 48
        },
        .banner_caption: {
            padding: [12, 24, 0, {if: =view.width, equals: 414, then: 106, else: {if: =view.width, equals: 320, then: 56, else: 94}}],
            font-name: WhitneySSm-Book,
            font-size: 16,
            line-height: 22,
            color: #3b4343,
            text-align: left
        },
        .cont_caption: {
            font-name: WhitneySSm-Book,
            font-size: 16,
            line-height: 44,
            color: #4f5757,
            position: absolute,
            anchor-y: top,
            padding: [-33, 0, 0, {if: =view.width, equals: 320, then: 60, else: 80}]
        },
        .notifications: {
            font-name: WhitneySSm-Book,
            font-size: 16,
            color: #4f5757,
            position: absolute,
            text-align: center,
            padding: [0, 24, 8, 34]
        },
        #bell: {
            background-image: "url(thickBell_promo@3x.png)",
            width: 24,
            height: 24,
            background-position: aspect-fit,
            position: absolute,
            top: 9,
            left: {if: =view.width, equals: 414, then: 140, else: {if: =view.width, equals: 320, then: 86, else: 124 }}

        }
    }
    </style>


    <box id="election">
    <panel id="election_container">
        <panel id="headline_container">
            <p id="banner_headline" class="banner_headline"><a id="banner_headline" href="subscribe:politics?tag-group=topic"><span class="banner_headline">FOLLOW ELECTION 2016</span><mark class="space"/></a></p>
        </panel>

        <panel id="bell-panel"><box id="bell" />
            <p id="banner_caption" class="banner_caption">Tap <mark class="banner_caption" /></p>
            <p id="cont_caption" class="cont_caption"> to manage your <mark class="cont_caption" /></p>
            <p id="notifications" class="notifications">notifications<mark class="notifications" /></p>
        </panel>
    </panel>
    </box>

	<p class="article_flashline" embed="flashline"/>

	<panel id="header_top">
		<p class="article_headline"><span embed="headline"/></p>
		<h2 class="article_subhed" embed="subhed" />
	</panel>

	<box id="media_container">
		<p id="media_title"><mark id="mark_media_title"/></p>
        <panel id="media_vr_header">
            <p id="media_vr_header_left"><mark id="mark_media_vr_header_left"/></p>
            <p id="media_vr_header_right"><mark id="mark_media_vr_header_right"/></p>
        </panel>
        <box id="media_asset" embed="media"/>
		<p id="media_vr_footer"><mark id="mark_media_vr_footer"/></p>
        <p id="media_caption"><mark id="mark_caption" /></p>
        <p id="media_vr_link"><a href="http://wsj.com/virtual-reality"><mark id="mark_media_vr_link"/></a></p>
    </box>

	<panel id="body_container">
		<p class="article_byline" embed="byline"></p>
		<p class="article_pubdate">Updated
			<span  embed="pubdate-string"/>
		</p>
		<panel class="article_body" embed="body"/>
	</panel>

</jpml>
