<jpml id="FRONT-3story-feature-no-ad_template">
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="GRID-stylesheet2.ejs"/>

	<style>{
		#page_details: {
			options: { hideHeaderBorder: true, tintTitle: true }
		}
	}</style>

	<style> {
		$switch: [
			{if: =portrait, then: {
				#b1:	{
					options:{ large_headline: true, caption_position: 6, caption_size: 1, top_border: true }
				},
				#b2:	{
					options:{ medium_headline: true, show_image: true, small_image: true, show_flashline: true, show_byline: true, show_subhead: false, top_border: true, body_column_count: 2 }
				},
				#b3:	{
					options:{ medium_headline: true, show_image: true, small_image: true, show_flashline: true, show_byline: true, show_subhead: false, top_border: true, body_column_count: 2 }
				}
			}},
			{if: =landscape, then: {
				#b1:	{
					options:{ large_headline: true, caption_position: 8, caption_size: 2, top_border: true }
				},
				#b2:	{
					options:{ medium_headline: true, show_image: true, small_image: true, show_flashline: true, show_byline: true, show_subhead: false, top_border: true, module: url(TOP-IMAGE_module3.xml) }
				},
				#b3:	{
					options:{ medium_headline: true, show_image: true, small_image: true, show_flashline: true, show_byline: true, show_subhead: false, top_border: true, module: url(TOP-IMAGE_module3.xml) }
				}
			}}
		]
	} </style>

	<box id="page_details" embed="pagedescription" />
	<box id="b1" embed="article" class="portrait-xposition-1 portrait-yposition-5 portrait-columns-12 portrait-lines-29 landscape-xposition-1 landscape-yposition-5 landscape-columns-13 landscape-lines-31" module="DECO_SOLID_CAPTION_module.xml" />

	<box id="b2" embed="article" class="portrait-xposition-1 portrait-yposition-35 portrait-columns-6 portrait-lines-14 landscape-xposition-14 landscape-yposition-5 landscape-columns-3 landscape-lines-16" module="BOTTOM-INSET-IMAGE_module.xml" />

	<box id="b3" embed="article" class="portrait-xposition-7 portrait-yposition-35 portrait-columns-6 portrait-lines-14 landscape-xposition-14 landscape-yposition-22 landscape-columns-3 landscape-lines-15" module="BOTTOM-INSET-IMAGE_module.xml" />

	<style>{
		#next_page: {
			top: {if: =portrait, then: 990, else: 733}
		}
	}</style>

	<box id="next_page" class="tablet_only paged_only landscape-columns-16 portrait-columns-12 xposition-1">
		<box id="next_page_button" embed="next-page-arrow"/>
	</box>
</jpml>
