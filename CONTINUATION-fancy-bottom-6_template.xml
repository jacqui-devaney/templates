<jpml id="CONTINUATION-fancy-bottom-6_template">
	<style src="CONTINUATION-stylesheet.ejs" /> 
	<style src="GLOBAL-stylesheet.ejs" />
	<style>{
        if: =portrait, then: {
	        #b1: { 
					frame: {$switch: [
						{if: =scale.small, then: [0, 0, 256, 528]},
						{if: =scale.large, then: [0, 0, 256, 478]}
					]}
			},
	        #b2: { 
					frame: {$switch: [
						{if: =scale.small, then: [0, 529, 256, 346]},
						{if: =scale.large, then: [0, 479, 256, 396]}
					]},
					options: {hide_strap: {if:=articles.b2.subsection, equals:=articles.b1.subsection}} 
			},
	        #b3: { 
					frame: {$switch: [
						{if: =scale.small, then: [256, 0, 256, 528]},
						{if: =scale.large, then: [256, 0, 256, 478]}
					]},
					options: {hide_strap: {if:=articles.b3.subsection, equals:=articles.b2.subsection}} 
			},
	        #b4: { 
					frame: {$switch: [
						{if: =scale.small, then: [512, 0, 256, 264]},
						{if: =scale.large, then: [512, 0, 256, 239]}
					]},
					options: {hide_strap: {if:=articles.b4.subsection, equals:=articles.b3.subsection}} 
			},
	        #b5: { 
					frame: {$switch: [
						{if: =scale.small, then: [512, 264, 256, 264]},
						{if: =scale.large, then: [512, 239, 256, 239]}
					]},
					options: {hide_strap: {if:=articles.b5.subsection, equals:=articles.b4.subsection}} 
			},
            #deco: { 
						frame: {$switch: [
							{if: =scale.small, then: [264, 529, 491, 345]},
							{if: =scale.large, then: [264, 479, 491, 395]}
						]},
						options: {normal_headline:true} 
			}
        }, else: {
	        #b1: { frame: [0, 0, 256, 619] },
	        #b2: { frame: [256, 0, 256, 224], options: {hide_strap: {if:=articles.b3.subsection, equals:=articles.b2.subsection}} },
	        #b3: { frame: [256, 232, 256, 395], options: {hide_strap: {if:=articles.b2.subsection, equals:=articles.b1.subsection}} },
	        #b4: { frame: [512, 0, 256, 224], options: {hide_strap: {if:=articles.b4.subsection, equals:=articles.b3.subsection}} },
	        #b5: { frame: [768, 0, 256, 224], options: {hide_strap: {if:=articles.b5.subsection, equals:=articles.b4.subsection}} },
	        #deco: { frame: [512, 225, 512, 395], options: {normal_headline:true, F_image:{if: =widescreen, then:true, else:false}} }
        }
    }</style>
    <box id="b1" embed="article" module="CONTINUATION-dynamic_module.xml" />    
    <box id="b2" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="b3" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="b4" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="b5" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="deco" embed="article" module="DECO_module.xml" />

</jpml>
