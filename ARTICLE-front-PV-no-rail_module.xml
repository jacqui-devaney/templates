<jpml id="ARTICLE-front-PV-no-rail_module">
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="GLOBAL-stylesheet.ejs" />
	<style>{
		$join: [
		{#header_top: { if: =portrait, then: { padding:[660, 0, 0, 7]}, else: { padding:[35, 0, 0, 740]}}},
		{#body_container: { 
			padding:{if: =portrait, then: [4, 15, 0, 7], else: [4, 15, 0, 22]},
            column-count:{if: =portrait, then:{$divide: [=view.width, {$multiply:[260, =scale.dpi]}]}, else: 1},
            column-gutter: 30,
			column-height:balance,
			flow: article_body
        }},
		{#mediabucket: {frame: [0, 0, 720, 600] }},
		{if: =portrait, then: {
            #mediabucket_container:	{frame: [15, 40, 718, 600]}
        }, else: {
        	#mediabucket_container:	{ frame: [0, 34, 718, 600]}
        }},
    	
    	{#this_close_button_box: {
	        border-image: url(close.png),
	        border-slice: [6],
			border-width: [6],
			border-color:#fff,
			width: 48,
			height:19,
			right:0
			
		}},
		{#this_close_button_p: {
			text-align: center,
			font-size:11,
			color:#333,
			font-name:Arial-BoldMT,
			padding:[4, 0, 0, 0]
		}}

		]
    }</style>
	<box id="this_close_button_box" embed="close">
		<p id="this_close_button_p">Close</p>
	</box>
	<box id="mediabucket_container">
		<box id="mediabucket" embed="mediabucket"></box>
	</box>
	<panel id="header_top">
        <p class="offduty_article_headline"><span embed="headline"/><mark class="key"/></p>
		<h2 class="offduty_article_subhed" embed="subhed" />
    </panel>
	<panel id="body_container">
	    <p class="article_byline" embed="byline"/>
        <panel class="article_body" embed="body"/>
    </panel>
	
</jpml>


