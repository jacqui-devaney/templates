{
	jpml: {
		padding:[0]
	},
	.article_headline: {
		font-name: ChronicleDisplay-Semibold,
		font-size: 28,
		line-height:32,
		color:#2b3333,
		padding:[0, 0, 5, 0]
	},
	.article_subhed: {
		font-name: Whitney-Book,
		text-align:left,
		font-size:28,
		line-height:32,
        color:#2b3333
	},
	.module_flashline: {
        font-name: Whitney-Semibold,
		font-size:{$multiply: [12, =view.zoomlevel]},
		line-height:{$multiply: [13, =view.zoomlevel]}
	},
	.byline: {
		font-name: Whitney-Book,
		font-size:{$multiply: [17, =view.zoomlevel]},
		line-height:{$multiply: [22, =view.zoomlevel]},
		color:#a7afaf
	},
	.border-p:{
		padding: [0, 0, 22, 0]
	},
	.border-mark: {
		font-size: 4,
		line-height:4,
		color: transparent,
		content: "unfortunate"

	},
	.byline-author: {
		font-name: ChronicleDisplay-Italic,
        font-size: 15,
        line-height: 22,
        color: #2fbfbf
	},
	.h2: {
        font-name:{if: =intent.article, then:Whitney-Book, else:Whitney-Medium},
		text-indent:0
	},
	.article_body: {
		font-name: ChronicleDisplay-Roman,
		font-size:{$multiply: [15, =view.zoomlevel]},
		line-height:{$multiply: [22, =view.zoomlevel]},
		color:#3b4343
	},
	.body-last-p-mark: {
		display: {if:=intent.article, then:default, else:none},
		font-name: "DJ5Utility-Regular",
		font-size:{$multiply: [15, =view.zoomlevel]},
		content: " Y"
	},
	strong: {
    font-name:{if: =intent.article, then:ChronicleDisp-Bold, else:ChroniceDisp-Black}
	},
	em: {
    font-name:{if: =intent.article, then:ChronicleDisp-BlackItalic, else:ChronicleDisp-Black}
	},
	cite: {
		font-name:{if: =intent.article, then:ChronicleDisp-BlackItalic, else:ChronicleDisp-Black}
	},
	#mediabucket_container: {
		display: {if: =page.mediabucket, then:inherit, else:none}
	},
	.byline-credit: {
		display: {if-any: [
			{if: =page.section, equals: "CIO", then:true},
			{if: =page.section, equals: "CFO", then:true}
		],
		then: none,
        else: inherit},
        font-name:Whitney-Book,
        color: #a7afaf,
        font-size: 13,
        line-height: 22,
        text-transform: uppercase
	},
	#m_subsection: { content: =article.subsection },
	#m_headline: { content: =article.headline },
	.list-item: {
		text-indent: -16,
		padding: [0, 0, 0, 16]
	},
	.list-item-bullet: {
		content: "\u00a0\u2022 "
	},
	.blockquote: {
		padding: [0, 0, 10, 20]
	},
	.blockquote-p: {
		padding: [0, 0, 0, 8],
		border-width: [0, 0, 0, 1],
		border-color: black
	},
	.ticker: {
		display: inherit,
		color: transparent,
		hyphens: none
	},
	.tweet: {
		display: {if: =app.version, equals: "17.0", then: inherit, else: none}
	},
	.tweet-p: {
		padding: [0],
		break-inside: avoid-column,
		text-align: justify,
		text-indent: 0,
		line-height: 90
	},
	.tweet-mark: {
		font-name: "HelveticaNeue",
		font-size: 60,
	    hyphens: none,
	    color: transparent,
		content: "TWTR-TWTR-"
	},
	#media: {
		display: {if: =page.hasmedia, then: inherit, else: none},
		padding: [4, 4, 4, 4]
	},
	#media_caption: {
        color: #4f5757,
        font-size: 17,
        line-height: 22,
        padding: [8, 16, 22, 16],
		font-name: "Whitney-Book"
	},
	#mark_caption: {
		content: =media.caption
	},
	#media_asset: {
		position: relative,
		left: 0,
		right: 0,
		height: {$min: [{$max: [{$switch: [
			{if: =media.width, less-than: 360, then: {$divide: [236, =media.aspect]}},
			{if: =media.width, more-than: 484, then: {$divide: [484, =media.aspect]}},
			{$divide: [236, =media.aspect]}
		]}, {$multiply: [.1, =view.height]}]}, {$multiply: [.69, =view.height]}]},
		options: {
			id: {
				if: =page.mediaindex,
				at-least: 0,
				then: =page.mediaindex,
				else: =page.number
			},
			hideCaption: true
		},
		padding: [4, 4, 4, 4]
	}
}
