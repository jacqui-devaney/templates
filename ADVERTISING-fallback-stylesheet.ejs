{
	$join: [
		{
			if: =app.version, less-than: "7.7", then: {
				if: =scale.phone, then:{
					#cube_ad_container: {
						position: { if: =intent.section, then: absolute, else: relative },
						top: 0,
						left: 0,
						right: 0,
						height: 342,
						background-color: #ffffff,
						display: { if: =scale.rail, then: none, else: inherit }
					},
					#cube_ad: {
						border-width: [46],
						border-color: #ffffff,
						anchor-x:center,
						display: inherit,
						pixels-per-point-x: 1,
						pixels-per-point-y: 1,
						width: 300,
						height: 250,
						options: {
							excludedLineItemIds: ["383422586", "383422226", "383421746", "440015306"]
						}
					},
					#ad_headline:{
						font-name: "WhitneySSm-Book",
						font-size: 13,
						line-height: 45,
						color: #a7afaf,
						display: inherit,
						text-align: center,
						padding: [-43, 40, 10, 40]
					}
				}, else: {
					#front_page: {
						display: inherit,
						position: inherit,
						top: {if: =landscape, then:568, else:824},
						height: 90
					},
					#banner_ad: {
						pixels-per-point-x: 1,
						pixels-per-point-y: 1,
						anchor-x:center,
						anchor-y:middle,
						width: =view.width,
						height: 90,
						options: {
							excludedLineItemIds: ["383422586", "383422226", "383421746", "440015306"]
						}
					},
					#fullpage_ad: {
						anchor-x:center,
						display: inherit,
						pixels-per-point-x: 1,
						pixels-per-point-y: 1,
						width: =view.width,
						height: =view.height
					},				
					#article_advertisement: {
						pixels-per-point-x: 1,
						pixels-per-point-y: 1,
						width:484,
						top:0,
						bottom:0,
						right:0,
						border-width:[0, 0, 0, 1],
						border-color:#d8d8d8
					},
					#skyscraper_ad:{
						width:300,
						height:600,
						anchor-x:center,
						anchor-y:middle
					},
					#fullpage_container: {
						height: 270
					}
				}
			}
		},
		{
			if: =app.version, less-than: "7.7", then: {
				#lastpage_ad_container: {
					display: { if-any: [
						=page.hasmedia,
						{ if: =ad.type, not-equal-to: "Skyscraper", then: true, else: false },
						{ if: [1], contains: =page.number, then: true, else: false }
					], then: none },
					position: absolute,
					border-color: #d8d8d8,
					border-width: [0, 0, 0, 1],
					min-width: 300,
					min-height: 600,
					top: 0,
					bottom: 0,
					left: 239,
					right: 0
				}
			}, else: {
			}
		},
		{
			if: =app.version, less-than: "7.7", then: {
				if: =ad.type, equals: "Cube", then: {
					if: =intent.article, then: {
						if: =ad.loaded, then: {
							if: ["383422586", "383422226", "383421746", "440015306"], contains: =ad.lineItemID, then: {
								#small_ad_container: {
									height: 1
								},
								#cube_ad: {
									height: 1
								},
								#ad_headline: {
									display: none
								}
							}, else: {
								if: =app.version, less-than: "7.6", then: {
									#body_container: {
										display: none
									}
								}, else: {}
							}
						}, else: {
							#small_ad_container: {
								display: none
							}
						}
					}, else: {}
				}, else: {
					#small_ad_container: {
						display: none
					}
				}
			}, else: {
			}
		},
		{
			if: =app.version, less-than: "7.7", then: {
				if: =ad.type, equals: "Banner", then: {
				}, else: {
					#banner_ad: {
						display: none
					}
				}
			}, else: {
			}
		},
		{
			if: =app.version, less-than: "7.7", then: {
				if: =ad.type, equals: "Skyscraper", then: {
					if-any: [
						{ if: [3, 5, 7], contains: =page.number, then: true, else: false },
						{ if-all: [ {if: =page.number, equals: 1}, =ad.loaded], then: true, else: false }
					], then: {
						if: ["383422586", "383422226", "383421746", "440015306"], contains: =ad.lineItemID, then: {
							#article_advertisement: {
								position: absolute,
								height: 1,
								width: 1
							},
							#skyscraper_ad: {
								position: absolute,
								height: 1,
								width: 1
							}
						}, else: {
							#media: {
								display: none
							},
							#media_container: {
								display: none
							},
							.article_body: {
								column-count: {if: =landscape, then:2, else:1},
								padding: [0, 12, 0, 0]
							}
						}
					}, else: {
						#article_advertisement: {
							display: none
						}
					}
				}, else: {
					#article_advertisement: {
						display: none
					}
				}
			}, else: {
			}
		},
		{
			if: =app.version, less-than: "7.7", then: {
				if: =ad.type, equals: "Fullpage", then: {
					if-all: [
						{
							if: =app.version, less-than: "7.6", then: {
								if: =page.number, more-than: 1000000, then: true, else: false
							}, else: {
								if: =page.number, equals: 1, then: true, else: false
							}
						},
						=ad.loaded
					], then: {
						if: ["383422586", "383422226", "383421746", "440015306"], contains: =ad.lineItemID, then: {
							#fullpage_container: {
								position: absolute,
								height: 1,
								width: 1
							},
							#fullpage_ad: {
								position: absolute,
								height: 1,
								width: 1
							}
						}, else: {
							jpml: {
								min-height:0,
								padding: [0,0,0,0]
							},
							#fullpage_container: {
								display: inherit
							},
							#view: {
								display: { if: =app.version, less-than: "7.6", then: none, else: inherit },
								position: absolute,
								height: 200,
								width: 1
							},
							#media: {
								display: none
							},
							#media_container: {
								display: none
							},
							#page_details: {
								display: { if: =app.version, less-than: "7.6", then: none, else: inherit },
							    options: {
								fullPageAd: true
							    }
							}
						}
					}, else: {
						#fullpage_container: {
							display: none
						}
					}
				}, else: {
					#fullpage_container: {
						display: none
					}
				}
			}, else: {
			}
		}
	]
}
