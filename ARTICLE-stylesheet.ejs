{
	jpml: {
		padding:[0]
	},
	.article_headline: {
		font-name: "DJ5EscrowCond-Bold",
		font-size: 48,
		line-height:50,
		color:#000,
		padding:[0, 0, 5, 0]
	},
	.article_subhed: {
		font-name:DJ5EscrowCond-SemiboldItalic,
		text-align:left,
		font-size:25,
		line-height:30,
		color:#5d5d5d
	},
	.module_flashline: {
		font-size:{$multiply: [12, =view.zoomlevel]},
		line-height:{$multiply: [13, =view.zoomlevel]}
	},
	.byline: {
		font-name:DJ5Exchange-Semibold,
		font-size:{$multiply: [14, =view.zoomlevel]},
		line-height:{$multiply: [20, =view.zoomlevel]},
		color:#000
	},
	.border-p:{
		border-width:[4, 123, 0, 0],
		background-color: #e1e0df,
		border-color:white
	},
	.border-mark: {
		font-size: 4,
		line-height:4,
		color: transparent,
		content: "unfortunate"

	},
	.byline-author: {
		text-transform:uppercase
	},
	h2: {
		font-name:{if: =intent.article, then:DJ5Exchange-Bold, else:DJ5Exchange-Medium},
		text-indent:0
	},
	.article_body: {
		font-name: "DJ5Exchange-Medium",
		font-size:{$multiply: [14, =view.zoomlevel]},
		line-height:{$multiply: [20, =view.zoomlevel]},
	    text-align: justify,
	    hyphens:auto,
		color:#222221,
		text-indent:=text.indent
	},
	.body-last-p-mark: {
		display: {if:=intent.article, then:default, else:none},
		font-name: "DJ5Utility-Regular",
		font-size:{$multiply: [15, =view.zoomlevel]},
		content: " Y"
	},
	strong: {
		font-name:{if: =intent.article, then:DJ5Exchange-Bold, else:DJ5Exchange-Medium}
	},
	em: {
		font-name:{if: =intent.article, then:DJ5Exchange-MediumItalic, else:DJ5Exchange-Medium}
	},
	cite: {
		font-name:{if: =intent.article, then:DJ5Exchange-MediumItalic, else:DJ5Exchange-Medium}
	},
	#mediabucket_container: {
		display: {if: =page.mediabucket, then:inherit, else:none}
	},
	.byline-credit: {
		display: {if-any: [
			{if: =page.section, equals: "CIO", then:true},
			{if: =page.section, equals: "CFO", then:true}
		],
		then: none,
		else: inherit}
	},
	#m_subsection: { content: =article.subsection },
	#m_headline: { content: =article.headline },
	.list-item: {
		text-indent: -16,
		padding: [0, 0, 0, 16]
	},
	.list-item-bullet: {
		content: "\u00a0\u2022 "
	},
	.blockquote: {
		padding: [0, 0, 10, 20]
	},
	.blockquote-p: {
		padding: [0, 0, 0, 8],
		border-width: [0, 0, 0, 1],
		border-color: black
	},
	.ticker: {
		display: inherit,
		color: transparent,
		hyphens: none
	},
	.tweet: {
		display: {if: =app.version, equals: "17.0", then: inherit, else: none}
	},
	.tweet-p: {
		padding: [0],
		break-inside: avoid-column,
		text-align: justify,
		text-indent: 0,
		line-height: 90
	},
	.tweet-mark: {
		font-name: "HelveticaNeue",
		font-size: 60,
	    hyphens: none,
	    color: transparent,
		content: "TWTR-TWTR-"
	},
	#media: {
		display: {if: =page.hasmedia, then: inherit, else: none},
		padding: [0, 0, 5, 0]
	},
	#media_caption: {
		layout-grid-line: 1,
		layout-grid-line-offset: 0,
        color: #212121,
        font-size: 11,
        line-height: 15,
        font-name: "DJ5Exchange-Medium",
        border-width: [0, 0, 1, 0],
        border-color: #d8d8d8
	},
	#mark_caption: {
		content: =media.caption
	},
	#media_asset: {
		position: relative,
		left: 0,
		right: 0,
		height: {$min: [{$max: [{$switch: [
			{if: =media.width, less-than: 360, then: {$divide: [236, =media.aspect]}},
			{if: =media.width, more-than: 484, then: {$divide: [484, =media.aspect]}},
			{$divide: [236, =media.aspect]}
		]}, {$multiply: [.1, =view.height]}]}, {$multiply: [.69, =view.height]}]},
		options: {
			id: {
				if: =page.mediaindex,
				at-least: 0,
				then: =page.mediaindex,
				else: =page.number
			},
			hideCaption: true
		},
		padding: [0, 0, 8, 0]
	}
}
