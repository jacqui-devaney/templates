<jpml id="FOE-SECTION-3story-horizontal_template">
	<style src="FUTURE-stylesheet.ejs" />
	<style src="GLOBAL-stylesheet2.ejs"/>
    <style src="GRID-stylesheet2.ejs"/>
	<style>{
		#page_details: {
			options: {
		        hideHeaderBorder: true,
				tintTitle: true
			}
		}
	}</style>
	<style>{
		jpml: {
			background-color: =color.background
			},
    	#ad_placeholder: {
    		display: none
    	},
    	#marketstrap: {
    		display: none
    	},
		.center-me: {
			text-align:center,
			anchor-y: middle
		},
		a: {
			padding: [8, 0]
		},
		span: {
			padding: [3, 0]
		},
		.more-link: {
			font-name: "WhitneySSm-Book"
		}
	}</style>
	<style> {
		$switch: [
			{if: =scale.rail, then: {
				#footer:{
					display: none
				},
				#tabbar-container:{
					display: none
				}
			}},
		{if: =portrait, then: {
				#b1:	{
					options:{ future_large_headline:true, show_image:true, top_image:true, two_columns:true, column_gap: true, headline_padding:true, move_body:true, anchor_body:true, anchor_headline:true}
				},
				#b2:	{
					options:{ future_medium_light_headline:true, show_body:true, top_border:true, show_image:true, top_image: true, show_columns: true, body_padding:true}
				},
				#b3:	{
					options:{ future_medium_light_headline:true, show_body:true, show_image:true, tall_image:true, top_border:true, show_columns: true}
				}
			}},
			{if: =landscape, then: {
				#b1:	{
					options:{ future_large_headline:true, show_image:true, tall_image:true}
				},
				#b2:	{
					options:{ future_medium_light_headline:true, show_body:true, show_image:true, squarish_image:true}
				},
				#b3:	{
					options:{ future_medium_light_headline:true, top_border:false, show_image:true, small_image:true}
				}
			}}
		]
	} </style>

	<box id="b1" embed="article" class="portrait-columns-12 landscape-columns-10 portrait-xposition-1 landscape-xposition-1 portrait-yposition-5 landscape-yposition-5 portrait-lines-30 landscape-lines-33 maude unit_plain" module="FOE_module2.xml" />
	<box id="b2" embed="article" class="portrait-columns-6 landscape-columns-3 portrait-yposition-36 landscape-yposition-5 landscape-xposition-11 portrait-xposition-1 portrait-lines-15 landscape-lines-33  maude unit_plain" module="FOE-LEFT-IMAGE_module.xml" />
	<box id="b3" embed="article" class="landscape-columns-3 portrait-columns-6 landscape-yposition-5 portrait-yposition-36 portrait-xposition-7 landscape-xposition-14 portrait-lines-15 landscape-lines-33 maude unit_plain" module="FOE-LEFT-IMAGE_module.xml" />
</jpml>