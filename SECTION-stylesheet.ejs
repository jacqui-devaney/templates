{
	.line { background-color:#b1aca1 },
	
	.highlight_dark { background-color:#e4e4e4 },
	
	.highlight_white { background-color:#fff },
	
	.highlight_light { background-color:#f6f5f3 },
	
	{ if =portrait, then: {
		#h1: 	{ frame: [0, 620, 255, 255] },
		#h2: 	{ frame: [255, 620, 255, 255] },
		#had: 	{ frame: [510, 620, 255, 257] },
		#h3: 	{ frame: [0, 620, 510, 255] },
		#l1:	{ frame: [255, 620, 1, 255] },
		#l2:	{ frame: [510, 620, 1, 255] }
		
	}, else: {
		#h1: 	{ frame: [787, 3, 243, 215] },
		#h2: 	{ frame: [787, 215, 243, 206] },
		#had: 	{ frame: [787, 420, 243, 206] }
		#h3: 	{ frame: [787, 3, 243, 420] }
		#l1:	{ frame: [787, 215, 243, 1] },
		#l2:	{ frame: [787, 420, 243, 1] },
	
	},
}