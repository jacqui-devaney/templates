        <box class="ups_strap_image"/>
        <p id="ups_strap_p"><mark id="ups_strap_mark"/></p>
    </panel>
    <box id="view" embed="article" class="maude" module="ARTICLE-ups-front-phone_module.xml" />
</jpml>
