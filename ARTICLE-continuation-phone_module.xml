<jpml id="ARTICLE-continuation-phone_module">
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="ARTICLE-stylesheet.ejs" />
	<style src="PHONE-MODULES-stylesheet.ejs" />
	<style src="MEDIA-stylesheet.ejs" />
	<style>{
		$join: [
		{jpml: { min-height: 0 }},
		{.article_body: {
			line-height: {$multiply: [22, =view.zoomlevel]},
			text-align: left,
            hyphens: auto
        }},
		{#body_continuation: {
			content: from-flow(article_body),
			flow: article_body
        }},
        {#media_container: {
            padding: [0, 0, 22, 0]
        }}
		]
    }</style>

    <box id="prototype">
        <box id="media-item-prototype">
        	<box id="media_container">
        		<p id="media_title"><mark id="mark_media_title"/></p>
        		<p id="media_vr_header"><mark id="mark_media_vr_header"/></p>
	            <box id="media_asset" embed="media"/>
        		<p id="media_vr_footer"><mark id="mark_media_vr_footer"/></p>
	            <p id="media_caption"><mark id="mark_caption" /></p>
	            <p id="media_vr_link"><a href="http://wsj.com/virtual-reality"><mark id="mark_media_vr_link"/></a></p>
	        </box>
        </box>
    </box>

	<panel id="body_container">
    	<panel class="article_body" id="body_continuation" embed="body" />
	</panel>
</jpml>
