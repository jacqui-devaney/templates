<jpml id="NAVIGATION_module">
	<!-- This module has a bunch of gross negative padding.  Lets not do this -->
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="PHONE-MODULES-stylesheet.ejs" />
	<style>{
		jpml: {
			background-color: =options.background_color,
			padding:[{if: =view.height, less-than: =size.rail_height_with_image, then: 10, else: 4},25,{if: =view.height, less-than: =size.rail_height_with_image, then: -10, else: -2}, 25],
			height:130
		}
	}</style>
	<style>{
		#image_container: {
			display: { if: {$count:[=article.thumbnail]}, more-than: 0, then: inherit, else: none},
			anchor-x:left,
			anchor-y:left,
			padding:[5, 15, 0, 0],
			width: 160,
			height: 115
		},
		image: {
			options: {
				match: {
					max-width: 553
				},
				source: { 
					if: {$count:[=article.thumbnail]}, 
					more-than: 0, 
					then: { $join: [
						"url",
						"(",
						=article.thumbnail,
						")"
					] }
				}
			}
		},
		.byline: {
			display: {if: =options.show_byline, then:inherit, else:none}

		},
		.article_subhed: {
			display: {if: =options.show_subhed, then:inherit, else:none}
		},
		.body: {
			display: inherit
		}


	}</style>
	<style>{
		.article_subhed: {
			font-name: DJ5Exchange-MediumItalic,
			font-size:14,
			line-height:17,
			color: #666666,
			padding:[-2,0,0,0]
		},
		.module_headline: {
			font-name: DJ5Exchange-Semibold,
			font-size: 18,
			line-height:21
		},
		#module_text: {
			padding:[5,0,10,0]
		},
		.body: {
			font-name: DJ5Exchange-Medium,
			font-size:14,
			line-height:17,
			padding:[{if: =options.show_subhed, then:2, else:-4},0,0,0],
			color: #555555
		},
		.byline: {
			font-size:14,
			font-name: DJ5Exchange-Medium,
			line-height:16,
			text-transform:uppercase,
			padding:[-4,0,4,0],
			color: #666666
		}
	}</style>

	<box id="image_container">
		<box id="deco_container" embed="deco-image"><image embed="summary-image" /></box>
	</box>
	<panel id="module_text">
		<p id="headline" class="module_headline"><mark class="m_headline" /></p>
		<p class="article_subhed" embed="subhed" />
		<p class="byline" embed="byline"/>
		<panel class="body" embed="body"/>
	</panel>

</jpml>
