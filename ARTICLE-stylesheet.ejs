{
	jpml: {
		padding:[0]
	},
	.article_headline: {
		font-name: { $switch: [
				{if: =issue.edition, equals: "japan", then: HiraKakuProN-W6},
				{if: =issue.edition, equals: "korea", then: AppleSDGothicNeo-Bold},
				"DJ5EscrowCond-Bold"
			]
		},
		font-size:{ $switch: [
				{if: =issue.edition, equals: "japan", then: 43},
				{if: =issue.edition, equals: "korea", then: 43},
				"48"
			]
		},
		line-height:50,
		color:#000,
		padding:[0, 0, 5, 0]
	},
	.article_subhed: {
		font-name:DJ5EscrowCond-SemiboldItalic,
		text-align:left,
		font-size:25,
		line-height:30,
		color:#5d5d5d
	},
	.module_flashline: {
		font-size:{$multiply: [12, =view.zoomlevel]},
		line-height:{$multiply: [13, =view.zoomlevel]}
	},
	.byline: {
		font-name:DJ5Exchange-Semibold,
		font-size:{$multiply: [14, =view.zoomlevel]},
		line-height:{$multiply: [20, =view.zoomlevel]},
		color:#000
	},
	#border_box: {
		padding:[4, 0, 0, 0],
		width:236,
		height:9
	},
	#border: {
		background-color:#e1e0df,
		height:4,
		width:113
	},
	.byline-author: {
		text-transform:uppercase
	},
	h2: {
		font-name:{if: =intent.article, then:DJ5Exchange-Bold, else:DJ5Exchange-Medium},
		text-indent:0
	},
	.article_body: {
		font-name: { $switch: [
				{if: =issue.edition, equals: "japan", then: HiraKakuProN-W3},
				{if: =issue.edition, equals: "korea", then: AppleSDGothicNeo-Medium},
				"DJ5Exchange-Medium"
			]
		},
		font-size:{$multiply: [14, =view.zoomlevel]},
		line-height:{$multiply: [20, =view.zoomlevel]},
	    text-align: left,
		color:#222221
	},
	.body-bug: {
		display: {if:=intent.article, then:default, else:none},
		font-name: {if: =device.platform, equals: Android, then: DJ5Utility, else: DJ5Utility-Regular },
		font-size:{$multiply: [15, =view.zoomlevel]},
		color:=page.tint,
		content: " Y"
	},
	strong: {
		font-name:{if: =intent.article, then:DJ5Exchange-Bold, else:DJ5Exchange-Medium}
	},
	em: {
		font-name:{if: =intent.article, then:DJ5Exchange-MediumItalic, else:DJ5Exchange-Medium}
	},
	cite: {
		font-name:{if: =intent.article, then:DJ5Exchange-MediumItalic, else:DJ5Exchange-Medium}
	},
	#mediabucket_container: {
		display: {if: =page.mediabucket, then:inherit, else:none}
	},
	.byline-credit: {
		display: {if-any: [
			{if: =page.section, equals: "CIO", then:true},
			{if: =page.section, equals: "CFO", then:true}
		],
		then: none,
		else: inherit}
	},
	#m_subsection: { content: =article.subsection },
	#m_headline: { content: =article.headline },
	.list-item: { 
		text-indent: -16,
		padding: [0, 0, 0, 16]
	},
	.list-item-bullet: {
		content: "\u00a0\u2022 "
	},
	.blockquote: {
		padding: [0, 0, 10, 20]
	},
	.blockquote-p: {
		padding: [0, 0, 0, 8],
		border-width: [0, 0, 0, 1],
		border-color: black
	  },
	#continued: {
		content: "Continued",		
		font-name: DJ5Exchange-SemiboldItalic,
		font-size:11,
		line-height:20,
		color:#898b8e,
		text-align:left
	}
	
		
}
