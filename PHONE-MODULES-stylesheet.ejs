{if: =scale.phone, then: {

	.key: {
		font-name: "DJ5Utility-Regular",
		font-size:16,
		content: "\u00a0K",
		display: { if-all: [=issue.locked, =article.paid], then: default, else: none },
		color:#948464
	},
	.module_summary: {
		font-name: Whitney-Book,
		font-size:17,
		line-height:22,
		color:#3b4343
	},
	.module_headline: {
		$join: [
			{
                font-name: "ChronicleDisplay-Semi",
                padding:[{if: {$count: [=article.images.phone]}, more-than:0, then:0, else:12},24,0,0],
				color:{if: =article.read, then: color.articleReadColor}
			},
			{
				$switch: [
					{if: =options.small_headline, then: {
                        font-name: "ChronicleDisplay-Semi",
						font-size: 22,
						line-height: 32
					}},
					{
                    font-size:{
                        if: {$count:[=article.images.phone]},
                        more-than: 0,
                        then: 22,
                        else: 22},
						line-height: 32
					}
				]
			}
		]
	},
	.body: {
		font-size:16,
		line-height:24,
		color:{if: =article.read, then: color.articleReadColor}
	},
	.ahed_headline:{
		font-size:17,
		line-height:22,
		padding:[0, 0, 0, 0],
		text-overflow: =text.overflow,
		font-name: Whitney-Book,
		color:#333

	},
	.highlight_flashline: {
        font-name: Whitney-Semibold,
		font-size:16,
		line-height:18,
		color:#656565,
		paragraph-padding: [ 0, 0, 20, 0 ]
	},

	#deco_headline: {
		font-size:18,
		line-height:18
	},
	#deco_bottom_headline: {
		font-size:21,
		line-height:21
	},
	.deco_caption: {
		font-size:14,
		padding: [0, 16, 22, 16],
		line-height:14
	},
	#strap: {
		display: { if:=options.show_strap, then:default, else:none },
		padding: [8.5, 8, 8.5, 8],
		border-width: [0.5, 0, 0.5, 0],
		border-color: black
    },
	.subsection: {
		font-name: "DJ5Exchange-Bold",
		font-size: 17,
		line-height: 16,
		color: =page.tint,
		text-transform: uppercase
	},
	.strap: {
		font-size:15,
		line-height:16,
        text-transform:uppercase,
        border-width: [0, 0, 0.5, 0],
        border-color: #e1e0df

	},
	.highlight_flashline: {
		font-size:14,
		padding: [ 0, 0, 0, 0 ],
		line-height:14
	},
	.highlight_headline: {
		font-size:16,
		line-height:16,
		padding:[0],
		text-overflow: =text.overflow
	},
	.highlight_large_headline: {
		font-size:16,
		line-height:16,
		padding:[0],
		text-overflow: =text.overflow
	},
	.highlight_flashline_small: {
		font-size:14,
		line-height:14
	},
	.article_flashline: {
		font-size:13,
        line-height:22,
        font-name: Whitney-Semibold,
        color: #2fbfbf,
		text-transform:uppercase,
        padding: [22,16,18,16],
        border-width: [0, 0, 0.5, 0],
        border-color: #e1e0df
	},
	.article_headline: {
		font-size:28,
		line-height:32,
        font-name: "ChronicleDisplay-Semi"
	},
	.article_subhed: {
		font-name: Whitney-Book,
		font-size:17,
		line-height:22,
		color:#4f5757
	},
	.article_byline: {
		font-name: Whitney-Book,
		font-size:17,
		line-height:22,
        color: #a7afaf
	},
	.article_pubdate: {
		font-name: Whitney-Book,
		font-size:13,
        line-height:22,
        color: #a7afaf
	},
	.article_body: {
		font-size:{$multiply: [15, =view.zoomlevel]},
		line-height:{$multiply: [22, =view.zoomlevel]},
        padding: [0, 3.5, 11, 3.5],
        font-name: ChronicleSSm-Book,
        text-indent: -1
	},
	.border-mark: {
		line-height: 4,
		color: transparent,
		content: "unfortunate"
	},
	.border-p:{
        display:none
	},
	.border-flashline-mark: {
		line-height: 1,
		color: transparent,
		content: "unfortunate"
	},
	.border-flashline:{
		border-width:[0, 0, 0.5, 0],
		background-color: #e1e0df,
		border-color:black
	},
	.opinion_headline: {
		font-size:18,
		line-height:25,
		font-name:ChronicleDisplay-Italic,
		text-align:left
	},
	.opinion_dek: {
		font-size:14,
		line-height:17,
		text-align:left,
		text-overflow: =text.overflow
	},
	.tech_section: {
		font-size:22,
		line-height:23
	},
	.tagline: {
		font-size:11
	},
	#extra_small_headline: {
		font-size:20,
		line-height:20,
		padding:[2, 0, 0, 0]
	},
	.large_dek: {
		font-name:DJ5Exchange-Medium,
		text-indent:0,
		font-size:18,
		line-height:20,
		text-overflow: =text.overflow,
        text-align: left,
		color:#000
	},
	.vertical_center: {
		vertical-align:middle
	},
	.hidden_tablet: {
		display: inherit
	},
	.hidden_phone: {
		display: none
	},
	.body-paragraph: {
		page-break-after: always
	},
	.body-panel: {
		page-break-after: always
	},

	#media_asset: {
        padding: [0, 4, 4, 4],
		height: {$min: [{$divide: [=view.width, =media.aspect]}, {$subtract:[=view.height, 770]}]}
	},
	#media_caption: {
		padding: [0, 16, 22, 16]
	},

	#ad_inline_inner: {
		pixels-per-point-x: 1,
		pixels-per-point-y: 1,
		anchor-x: center,
		anchor-y: middle,
		options: { zone: {$join: [=ad.prefix, abanner]} },
		width: 320,
		height: 50,
		background-color: #e7e2d8
	},

	#ad_inline_outer: {
		pixels-per-point-x: 1,
		pixels-per-point-y: 1,
		position: relative,
		min-height: 100,
		height: 50,
		left: -12,
		top: -80,
		right: -12,
		padding: [10, 0],
		display: none
	},

	#ad_inline_panel: {
		pixels-per-point-x: 1,
		pixels-per-point-y: 1,
		padding: [10, 0],
		min-height: 50,
		display: none
	},

	.ticker: {
		display: {if: =intent.article, then: inherit, else: none},
		color: transparent
	}

}, else:{}}
