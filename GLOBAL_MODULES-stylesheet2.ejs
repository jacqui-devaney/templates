{
	#invisible: {
		display:none
	},
	.module_headline: {
		$join: [
			{
				font-name: {
					$switch: [
						{if: =issue.type, equals: "NOW", then: "ChronicleDisplay-Semi"},
						{if: =issue.type, equals: "CSUITE", then: "WhitneySSm-Medium"},
						{if: =options.small_headline, then: "ChronicleSSm-Medium"},
						{if: =options.smedium_headline, then: "ChronicleDisplay-Semi"},
						{if: =options.medium_headline, then: "ChronicleDisplay-Semi"},
						{if: =options.very_large_caps_headline, then: "ChronicleDisplay-Semi"},
						"ChronicleDisplay-Semi"
					]
				}
			},
			{
				color:{if: =options.white_text, then:#fff, else:#2b3333}
			},
			{
				$switch:[
					{if: =options.small_headline, then:{
						font-size: 13,
						line-height: 20,
						color:#3b4343
					}},
					{if: =options.smedium_headline, then:{
						padding:[0, 0, 2, 0],
						color:#2b3333,
						font-size: 20,
						line-height: 25
					}},
					{if: =options.medium_headline, then:{
						padding:[6, 0, 0, 0],
						color:#2b3333,
						font-size: 21,
						line-height: 25
					}},
					{if: =options.large_headline, then: {
						padding: [17, 0, 0, 0],
						color:#2b3333,
						font-size: 36,
						line-height: 40
					}},
					{if: =options.very_large_caps_headline, then: {
						padding:[0, 0, 20, 0],
						font-size: 56,
						line-height: 60,
						text-transform:uppercase
					}},
					{if: true, then: {
						padding:[0, 0, 20, 0],
						font-size:19,
						line-height: 23
					}}
				]
			}
		]
	},
	.module_summary: {
		content: { $switch: [=article.body_extract, =article.summary] }
	},
	.body: {
		font-name: ChronicleSSm-Book,
		text-indent: 0,
		font-size:{$multiply:{if: =options.large_body, then:20, else:13}},
		line-height:{$multiply:{if: =options.large_body, then:26, else:20}},
		text-overflow: =text.overflow,
		text-align: left,
		hyphens:auto,
		color:{if: =options.white_text, then:#fff, else:#3b4343}
	},
	.escrow_body: {
		font-name: DJ5Escrow-Roman,
		text-indent:0,
		font-size:24,
		line-height:26,
		text-align: { if: =options.justify, then:=justify, else:left},
		color:{if: =options.white_text, then:#fff, else:#222221}
	},
	.deco_caption: {
		font-name: ChronicleSSm-Book,
		font-size:13,
		line-height:20,
		color:#fff
	},
	.deco_headline:{
		font-name: ChronicleDisplay-Semi,
		padding:[0, 0, 2, 0],
		color:white,
		font-size: 20,
		line-height: 25
	},
	.module_flashline: {
		font-name:WhitneySSm-SemiBold,
		font-size:11,
		line-height:20,
		color: #a7afaf,
		text-transform:uppercase,
		padding:[0, 0, 3, 0]
	},
	.module_caption: {
		font-name:DJ5Exchange-Medium,
		font-size:11,
		line-height:13,
		padding:[4, 0, 4, 0],
		display: {if: =options.show_image, then:inherit, else:none}
	},
	.key: {
		font-name: "DJ5Utility-Regular",
		font-size: 16,
		content: "\u00a0K",
		display: { if-all: [=issue.locked, =article.paid], then: default, else: none },
		color: #948464
	},
	#body-first-p: {
		text-indent: 0
	},
	.body-first-p-mark: {
		font-name: "DJ5Utility-Regular",
		content: "K\u00a0",
		display: { if-all: [=issue.locked, =article.paid], then: default, else: none },
		color: #948464
	},
	.byline: {
		font-name:ChronicleSSmMedium-Italic,
		font-size:13,
		line-height:20,
		color:#a7afaf
	},
	.hidden_tablet: {
		display: none
	},
	.hidden_phone: {
		display: inherit
	},
	#m_subsection: { content: =article.subsection },
	.m_headline: { content: =article.headline},
	.body-mark: { content: { $switch: [=article.body_extract, =article.summary] } },
	.byline-mark: {	content: =article.byline },
	.flashline-mark: { content: =article.flashline },
	.subhed-mark: {
		content: =article.deckline
	},
	em: {
		font-name:DJ5Exchange-MediumItalic
	},
	.link-pdf: {
		color:{if: =intent.article, then:{if: =page.tint, equals: "rgba(0.000000, 0.000000, 0.000000, 1.000000)", then: #004990, else: =page.tint}}
	},
	.link-external: {
		color:{if: =intent.article, then:{if: =page.tint, equals: "rgba(0.000000, 0.000000, 0.000000, 1.000000)", then: #004990, else: =page.tint}}
	},
	a: {
		color:{if: =intent.article, then:{if: =page.tint, equals: "rgba(0.000000, 0.000000, 0.000000, 1.000000)", then: #004990, else: =page.tint}}
	},
	.link-article: {
		text-decoration: none,
		color:{if: =intent.article, then:{if: =page.tint, equals: "rgba(0.000000, 0.000000, 0.000000, 1.000000)", then: #004990, else: =page.tint}}
	},
	.ticker: {
		display: none
	},
	.tweet: {
		display: none
	},
	image: {
		background-color: #f3f3f3
	},
	#b1: { bind: {if: =articles.b1.no_binding, then: none} },
	#b2: { bind: {if: =articles.b2.no_binding, then: none} },
	#b3: { bind: {if: =articles.b3.no_binding, then: none} },
	#b4: { bind: {if: =articles.b4.no_binding, then: none} },
	#b5: { bind: {if: =articles.b5.no_binding, then: none} },
	.summary-image: {
		options: {
			$join: [{
				source: {
					$switch: [
						{$switch: [
							{if-all: [{if: =options.image_idiom, equals: portrait}, {$count: [=article.images.portrait]}], then:
								{$join: ["url(", =article.images.portrait, ")"]}
							},
							{if-all: [{if: =options.image_idiom, equals: landscape}, {$count: [=article.images.landscape]}], then:
								{$join: ["url(", =article.images.landscape, ")"]}
							},
							{if-all: [=scale.phone, {$count: [=article.images.phone]}], then:
								{$join: ["url(", =article.images.phone, ")"]}
							},
							{if-all: [=scale.rail, {$count: [=article.images.rail]}], then:
								{$join: ["url(", =article.images.rail, ")"]}
							},
							{if-all: [=scale.tablet, {$count: [=article.images.tablet]}], then:
								{$join: ["url(", =article.images.tablet, ")"]}
							}
						]},
						{$switch: [
							{if-all: [{if: =options.image_idiom, equals: portrait}, {$count: [=article.image_portrait]}], then:
								{$join: ["url(", =article.image_portrait, ")"]}
							},
							{if-all: [{if: =options.image_idiom, equals: landscape}, {$count: [=article.image_landscape]}], then:
								{$join: ["url(", =article.image_landscape, ")"]}
							},
							{if-all: [=scale.phone, {$count: [=article.image_phone]}], then:
								{$join: ["url(", =article.image_phone, ")"]}
							},
							{if-all: [=scale.rail, {$count: [=article.image_rail]}], then:
								{$join: ["url(", =article.image_rail, ")"]}
							},
							{if-all: [=scale.tablet, {$count: [=article.image_tablet]}], then:
								{$join: ["url(", =article.image_tablet, ")"]}
							}
						]}
					]
				}
			}]
		}
	}
}
