<jpml id="CONTINUATION-dynamic">
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="GLOBAL-stylesheet.ejs" />
    <style>{
		jpml: { padding: [ 0, 8, 0, 0] },
        #content: { padding: [4] },
		#strap: {
            display: { if:=options.hide_strap, then:none, else:default },
			padding: [4]
        },
		#image_container: { 
			anchor-x: right,
			width: 84,
			height: 84,
			padding: [ 0, 0, 4, 8 ]
		}
    }</style>
    
	<panel id="strap">
		<p class="subsection" embed="subsection" />
	</panel>
	<panel id="content">
	 	<box id="image_container">
			<image class="A_size" embed="summary-image"/>
		</box>
		<p class="continuation_headline"><span embed="headline" /><mark class="key" /></p>
		<panel embed="body" class="continuation_body"/>
	</panel>
</jpml>
