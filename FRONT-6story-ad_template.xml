<jpml id="FRONT-6story-ad_template">
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="GRID-stylesheet2.ejs"/>
	<style src="ADVERTISING-stylesheet.ejs"/>
	<style src="ADVERTISING-fallback-stylesheet.ejs" />
	<style src="ADVERTISING-stylesheet2.ejs"/>
	<style>{
		#page_details: {
			options: {
				hideHeaderBorder: true,
				tintTitle: true
			}
		}
	}</style>

	<style> {
		$switch: [
			{if: =portrait, then: {
				#b1:	{
					options:{ large_headline: true, show_image: true, show_flashline: true, show_byline: true, show_subhead: true, body_column_count: 2, module: url(TOP-IMAGE_module3.xml), top_border: true }
				},
				#b2:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true }
				},
				#b3:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true }
				},
				#b4:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true }
				},
				#b5: {
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true  }
				},
				#b6: {
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true  }
                }
			}},
			{if: =landscape, then: {
				#b1:	{
					options:{ large_headline: true, show_image: true, show_flashline: true, show_byline: true, show_subhead: true, module: url(RIGHT-IMAGE_module2.xml), top_border: true }
				},
				#b2:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true  }
				},
				#b3:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true  }
				},
				#b4:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true  }
				},
				#b5:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true }
				},
				#b6:	{
					options:{ medium_headline: true, show_image: false, show_flashline: true, show_byline: true, show_subhead: false, top_border: true  }
				}
			}}
		]
	} </style>

	<box id="page_details" embed="pagedescription" />
	<box id="b1" embed="article" class="portrait-xposition-1 portrait-yposition-5 portrait-columns-8 portrait-lines-29 landscape-xposition-1 landscape-yposition-5 landscape-columns-12 landscape-lines-17" module="TOP-IMAGE_module3.xml" />

	<box id="b2" embed="article" class="portrait-xposition-9 portrait-yposition-5 portrait-columns-4 portrait-lines-10 landscape-xposition-8 landscape-yposition-23 landscape-columns-3 landscape-lines-13" module="TOP-IMAGE_module3.xml" />

	<box id="b3" embed="article" class="portrait-xposition-9 portrait-yposition-16 portrait-columns-4 portrait-lines-8 landscape-xposition-11 landscape-yposition-23 landscape-columns-3 landscape-lines-13" module="TOP-IMAGE_module3.xml" />

	<box id="b4" embed="article" class="portrait-xposition-9 portrait-yposition-25 portrait-columns-4 portrait-lines-9 landscape-xposition-14 landscape-yposition-23 landscape-columns-3 landscape-lines-13" module="TOP-IMAGE_module3.xml" />

	<box id="b5" embed="article" class="portrait-xposition-7 portrait-yposition-35 portrait-columns-3 portrait-lines-14 landscape-xposition-13 landscape-yposition-5 landscape-columns-4 landscape-lines-8" module="TOP-IMAGE_module3.xml" />

	<box id="b6" embed="article" class="portrait-xposition-10 portrait-yposition-35 portrait-columns-3 portrait-lines-14 landscape-xposition-13 landscape-yposition-14 landscape-columns-4 landscape-lines-8" module="TOP-IMAGE_module3.xml" />

	<box id="advertisement_container" class="portrait-xposition-1 portrait-yposition-35 portrait-columns-6 portrait-lines-14 landscape-xposition-1 landscape-yposition-23 landscape-columns-7 landscape-lines-13">
		<p id="advertisement_paragraph"><mark id="advertisement_mark"/></p>
		<box id= "advertisement" embed="advertisement"/>
	</box>

	<style>{
		#next_page: {
			top: {if: =portrait, then: 990, else: 733},
		}
	}</style>

	<box id="next_page" class="tablet_only paged_only landscape-columns-16 portrait-columns-12 xposition-1">
		<box id="next_page_button" embed="next-page-arrow"/>
	</box>
</jpml>
