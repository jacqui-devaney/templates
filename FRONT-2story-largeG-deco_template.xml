		if: =portrait, then: {
			#b1:	{ 
					frame: {$switch: [
						{if: =widescreen, then: [194, 395, 285, 480]},
						{if: =scale.large, then: [190, 482, 285, 394]}
					]},
				options:{small_headline:true, byline:true} 
			},
			#deco:	{ 
					frame: {$switch: [
						{if: =widescreen, then: [195, 28, 569, 367]},
						{if: =scale.large, then: [195, 26, 569, 457]}
					]},
					options:{normal_headline:true, no_caption:true, no_border:true} 
