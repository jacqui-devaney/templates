			{if: =landscape, then: {normal_headline:true, no_border:{if: =scale.small, then:false, else:true}, no_indent:true, DV_image:{if: =scale.small, then:false, else:true}, F_image:{if: =scale.small, then:true, else:false}, long_caption:true, no_caption:{if: =scale.small, then:true, else:inherit}}}		
		]}}
	}</style>
