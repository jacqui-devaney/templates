{
    #advertisement: {
        anchor-x:center,
        anchor-y:bottom,
        display: inherit,
        width: 300,
        height: 250,
        options: {
            adType: "Cube"
        },
        padding: [2, 0, 2, 0]
    },
    #advertisement_container: {
        display: inherit,
        border-color: #d8d8d8,
        border-width: [2, 0, 0, 0]
    },
    #advertisement_mark: {
        content: "ADVERTISEMENT"
    },
    #advertisement_paragraph: {
        font-name: "WhitneySSm-Semibold",
        color: #a7afaf,
        font-size: 12,
        text-indent: 0,
        display: inherit,
        text-align: center
    }
}
