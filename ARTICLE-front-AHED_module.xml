<jpml id="ARTICLE-front-AHED_module">
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="GLOBAL-stylesheet.ejs" />
	<style>{
		$join: [
		{#header_top: { padding:[18, 10, 8, 15]}},
		{.article_subhed: {font-size:18, line-height:21}},
		{if: =portrait, then: {
            #mediabucket_container:	{ 
				frame: [10, 10, 749, 528]
			},
            #head:{ padding: [10, 180, 0, 0]},
        	#article_copy:{ padding: [0, 180, 0, 0], max-height:250},
            #body_container:{ column-count:2, column-gutter: 20,             
				padding: [10, 0, 0, 20],
				column-height:balance,
				flow: article_body,
				layout-grid-line: {$multiply: [22, =view.zoomlevel]},
				layout-grid-line-offset: {$multiply: [20, =view.zoomlevel]}},
			#this_continues_label: {
				top: 875,
				right: 175,
				width:120,
				padding: [0, 15, 8, 0],
				display: { if: =page.count, equals:1, then:none, else:inherit}
			},
			#mediabucket: {
				width:749,
				height:522,
				anchor-x:center
			},
			.article_headline: {font-size:40,line-height:42}
		
		}, else: {
        	#mediabucket_container:	{ frame: [10, 110, 749, 512]},
        	#mediabucket: {
				width:749,
				height:512,
				anchor-x:middle
			},
        	#head:{ padding: [0, 0, 0, 0], max-height:130},
        	#article_copy:{ padding: [250, 0, 0, 770], max-height:285},
        	#body_container:{ padding: [10, 15, 10, 5],
				column-height:balance,
				flow: article_body,
				layout-grid-line: {$multiply: [22, =view.zoomlevel]},
				layout-grid-line-offset: {$multiply: [20, =view.zoomlevel]}
			},
			#this_continues_label: {
				top: 620,
				right: 0,
				width:120,
				padding: [0, 15, 8, 0],
				display: { if: =page.count, equals:1, then:none, else:inherit}
			},
			.article_headline: {font-size:36, line-height:38},
			.hr: {
				right:0
			}
        }}
	]
    }</style>
	<box id="this_continues_label">
		<p><mark id="continues" /></p>
	</box>
	<box id="mediabucket_container">
		<box id="mediabucket" embed="mediabucket" />
	</box>
	<panel id="head">
		<panel id="header_top">
			<p class="article_headline"><span embed="headline"/><mark class="key"/></p>
			<h2 class="article_subhed" embed="subhed" />
		</panel>
		<box class="hr"></box>
	</panel>
	<panel id="article_copy">
		<panel id="body_container">
			<p class="article_byline" embed="byline"/>
			<panel class="article_body" embed="body"/>
		</panel>
	</panel>
	
</jpml>


