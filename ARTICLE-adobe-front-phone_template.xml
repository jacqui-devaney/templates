<jpml id="ARTICLE-adobe-front-phone_template">
	<!-- GLOBAL not included -->
	<style src="PHONE-stylesheet.ejs" />
    <style src="adobe-stylesheet.ejs" />
	<style>{
		#view: {
			width:=view.width,
			height:{$multiply: [2, =view.height]},
			options: {
				indent: true
			}
		},
		#adobe_strap:{
			padding: [0, 12, 0, 12]
		}
	}</style>
    <panel id="adobe_strap">
        <box class="adobe_strap_image"/>
        <p id="adobe_strap_p"><mark id="adobe_strap_mark"/></p>
    </panel>
    <box id="view" embed="article" class="maude" module="ARTICLE-adobe-front-phone_module.xml" />
</jpml>
