<jpml id="CONTINUATION-AHED_template">
	<style src="GRID-stylesheet.ejs" /> 
	<style src="CONTINUATION-stylesheet.ejs" /> 
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="PHONE-stylesheet.ejs" /> 
    <style>{
		#b1: {options: { $switch: [
				{if: =scale.phone, then: {bottom_border:true}},
				{if: =portrait, then: {large_ahed:true, large_subhed:true, two_columns:true, center_image:true, indent:true, justify:true}},
				{if: =landscape, then: {large_ahed:true, large_subhed:true, two_columns:true, center_image:true, indent:true, justify:true}}			
		]}},
		#b2: {options: { $switch: [
				{if: =scale.phone, then: {}},
				{if: =portrait, then: {small_headline:true, no_byline:true, show_image:true, DV_image:true, left_border:true}},
				{if: =landscape, then: {small_headline:true, no_byline:true, show_image:true, DV_image:{if: =widescreen, then:false, else:true}, left_border:true}}			
		]}},
		#b3: {options: { $switch: [
				{if: =scale.phone, then: {}},
				{if: =portrait, then: {E_image:true, white_highlight:true, top_border:true}},
				{if: =landscape, then: {D_image:true, white_highlight:true, top_border:true, large_padding:true}}			
		]}},
		#b4: {options: { $switch: [
			{if: =scale.phone, then: {}},
			{if: =portrait, then: {E_image:true, white_highlight:true, top_border:true}},
			{if: =landscape, then: {D_image:true, white_highlight:true, top_border:true, large_padding:true}}			
		]}}
    }</style>
	<style>{$switch: [
        {if: =scale.phone, then: {
			#b3: {height:50},
			#b4: {height:50}
		}},
        {if: =portrait, then: {
            #b1 : { 
					height:551,
					top:8
			},
            #b2 : { 
				height:551,
				top:8
			},
            #b3 : { 
				height:402,
				top:559
			},
            #b4 : { 
				height:402,
				top:559
				
			}
	    }}, 
		{if: =landscape, then: {
	        #b1 : { 
					height:698,
					top:8
			},
	        #b2 : { 
				height:698,
				top:8
			},
	        #b3 : { 
				height:349,
				top:8
			},
	        #b4 : { 
				height:349,
				top:357
			
			}
        }}
    ]}</style>
    <box id="b1" embed="article" class="column4 positionx1 maude unit_continuation_strap" module="AHED_module.xml" />    
    <box id="b2" embed="article" class="column2 positionx5 maude unit_continuation_strap" module="TOP-IMAGE_module.xml" />
    <box id="b3" embed="article" class="column2-3 positionx7-1 maude unit_continuation_strap" module="SKYBOX_module.xml" />
    <box id="b4" embed="article" class="column2-3 positionx7-4 maude unit_continuation_strap" module="SKYBOX_module.xml" />
</jpml>

