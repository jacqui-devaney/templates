{
	#deloitte_strap:{
		min-height: 40,
		max-height: 40,
		background-color: #E7EDF0,
		padding: [0, 18, 0, 18]
	},
	#deloitte_strap_p:{
		text-align: right,
		padding: [14, 0, 0, 0]
	},
	#deloitte_strap_mark:{
		font-name: "Whitney-Semibold",
		font-size: 12,
		color: #333333,
		content: "CONTENT FROM OUR SPONSOR"
	},
	.deloitte_strap_image:{
		background-image: "url(deloitte@2x.png)",
		background-position: aspect-fit,
		top: 10,
		left: 0,
		width: 103,
		height: 20
	},

	#deloitte_list: {
		min-height: 0,
		max-height: 10000,
		background-color: #e8eeee
	},

	#deloitte_inner: {
		padding: [0, 15]
	},

	#deloitte_phone: {
		background-color: #e8eeee,
		padding: [0, 10]
	},

	.deloitte_item: {
		padding: { if: =scale.phone, then: [15, 0, 14.5, 0], else: [20, 0, 19.5, 0] },
		border-width: [0, 0, 0.5, 0],
		border-color: #979797
	},

	.deloitte_item_first: {
		if: =scale.phone, then: {
			padding: [14.5, 0, 14.5, 0],
			border-width: [0.5, 0, 0.5, 0],
			border-color: #979797
		}, else: {
			padding: [14.5, 0, 19.5, 0],
			border-width: [0.5, 0, 0.5, 0],
			border-color: #979797
		}
	},

	.deloitte_item_last: {
		padding: { if: =scale.phone, then: [15, 0, 15, 0], else: [20, 0, 20, 0] },
		border-width: [0]
	},

	.deloitte_headline: {
		font-name: "Whitney-Medium",
		font-size: 20,
		line-height: 25,
		color: #10427E,
		padding: [0, 0, 5, 0]
	},
	.deloitte_summary: {
		font-name: "Whitney-Book",
		font-size: { if: =scale.phone, then: 16, else: 14 },
		line-height: { if: =scale.phone, then: 25, else: 18 },
		color: #222221
	},

	.body-last-p-mark: {
			display: none
	},

	.article_body: {
		text-align:left,
		text-indent:0
	},

	h2: {
		text-indent:0
	},

	h3: {
		text-indent: 0,
		text-align: center
	},
	
	p: {
		padding:[0,0,20,0]
	},

	#body-first-p: {
		padding:[0,0,20,0]
	},

	#body-last-p: {
		padding: [0, 0, 8, 0]
	},

	.DELOITTEFOOTNOTES: {
		padding: [13, 0, 10, 0]
	},

	.DELOITTERELATEDCONTENT: {
		padding: [12, 0, 0, 0]
	},

	.DELOITTEQUESTIONS: {
		padding: [10, 0, 8, 0]
	},

	.DELOITTEDISCLAMER: {
		padding: [13, 0, 0, 0]
	},

	.deloitte-divider-p: {
		padding: [0, 0, 0, 0],
		border-width: [1, 0, 0, 0],
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		background-color: transparent,
		border-color: #D8D8D8
	},

	.deloitte-divider-mark: {
		font-size: 1,
		line-height: 1,
		color: transparent,
		content: "divider"
	},

	.deloitte-footnotes: {
		font-name: "Whitney-Medium",
		font-size: { if: =scale.phone, then: {$multiply: [14, =view.zoomlevel]}, else: {$multiply: [11, =view.zoomlevel]} },
		line-height: { if: =scale.phone, then: {$multiply: [18, =view.zoomlevel]}, else: {$multiply: [15, =view.zoomlevel]} },
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		color: #898B8E
	},

	.deloitte-related-content: {
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		font-name: "Whitney-Medium",
		color: #222222
	},

	.deloitte-questions: {
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		font-name: "Whitney-Medium",
		color: #222222
	},

	.deloitte-disclaimer: {
		font-name: "Whitney-Medium",
		font-size: { if: =scale.phone, then: {$multiply: [14, =view.zoomlevel]}, else: {$multiply: [11, =view.zoomlevel]} },
		line-height: { if: =scale.phone, then: {$multiply: [18, =view.zoomlevel]}, else: {$multiply: [15, =view.zoomlevel]} },
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		color: #222222
	},

	#deloitte_item_1: { if: true, then: { options: { reference: =articles.d1.id } } },
	#deloitte_item_2: { if: true, then: { options: { reference: =articles.d2.id } } },
	#deloitte_item_3: { if: true, then: { options: { reference: =articles.d3.id } } },
	#deloitte_item_4: { if: true, then: { options: { reference: =articles.d4.id } } },
	#deloitte_item_5: { if: true, then: { options: { reference: =articles.d5.id } } },
	#deloitte_item_6: { if: true, then: { options: { reference: =articles.d6.id } } },

	#deloitte_headline_1: { content: =articles.d1.headline },
	#deloitte_headline_2: { content: =articles.d2.headline },
	#deloitte_headline_3: { content: =articles.d3.headline },
	#deloitte_headline_4: { content: =articles.d4.headline },
	#deloitte_headline_5: { content: =articles.d5.headline },
	#deloitte_headline_6: { content: =articles.d6.headline },

	#deloitte_summary_1: { content: { $switch: [=articles.d1.websummary, =articles.d1.summary] } },
	#deloitte_summary_2: { content: { $switch: [=articles.d2.websummary, =articles.d2.summary] } },
	#deloitte_summary_3: { content: { $switch: [=articles.d3.websummary, =articles.d3.summary] } },
	#deloitte_summary_4: { content: { $switch: [=articles.d4.websummary, =articles.d4.summary] } },
	#deloitte_summary_5: { content: { $switch: [=articles.d5.websummary, =articles.d5.summary] } },
	#deloitte_summary_6: { content: { $switch: [=articles.d6.websummary, =articles.d6.summary] } }

}