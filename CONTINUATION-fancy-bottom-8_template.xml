            #b2 : { 
					frame: {$switch: [
						{if: =scale.small, then: [0, 528, 256, 345]},
						{if: =scale.large, then: [0, 479, 256, 396]}
					]},
					options: {hide_strap: {if:=articles.b2.subsection, equals:=articles.b1.subsection}} 
			},
            #b3 : { 
					frame: {$switch: [
						{if: =scale.small, then: [256, 0, 256, 264]},
