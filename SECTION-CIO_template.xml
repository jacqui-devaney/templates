	<style>{
		#cio: {
			display:{if: =page.section, equals: "CIO", then:inherit, else:none}
		},
		#cfo: {
			display: {if: =page.section, equals: "CFO", then:inherit, else:none}
		},
		#risk: {
			display: {if: =page.section, equals: "RISK_AND_COMPLIANCE", then:inherit, else:none}
		},
		.rando_heading: {
			font-size:18
		},
		.ahed_headline: {
			font-size:{if: =scale.phone, then: 10, else:16},
			line-height:{if: =scale.phone, then: 10, else:16},
			font-name:DJ5EscrowCond-BoldItalic,
			padding: [5, 0, 0, 0]
		},
		#stars: {
			padding:[5, 0, 0, 0]
			
		},
		.ahed_stars: {
			content: "*\u00a0\u00a0*\u00a0\u00a0*",
			font-size:{if: =scale.phone, then: 10, else:16}
		},
		
		#b1:	{ 
			options: {if: =scale.phone, then: 
				{show_body:true, no_border:true, large_headline:true, show_image:false, top_align:true}, else:
				{medium_headline:true, no_border:true}
			}
		},
		#b2:	{ 
			options: {if: =scale.phone, then:
				{small_headline:true, hide_image:true}, else:
				{small_headline:true}
			}
		},
		#b3:	{ 
			options: {if: =scale.phone, then:
				{small_headline:true, hide_image:true}, else:
				{small_headline:true}
			}
		},
		#b5:	{ 
			options: {if: =scale.phone, then:
				{small_headline:true, no_border:true, hide_image:true}, else:
				{small_headline:true, C_image:true}
			}
		},
		#b6:	{ 
			options: {if: =scale.phone, then:
				{small_headline:true, no_border:true, top_border:true, hide_image:true}, else:
				{small_headline:true, border:true}
			}
		},
		#b7:	{ 
			options: {small:true, border:true}
		},
		#b8:	{ 
			options: {if: =scale.phone, then:
				{small:true, border:true}, else:
				{small:true}
			}
		},
		#b9:	{ 
			options: {if: =scale.phone, then:
				{small:true, border:true}, else:
				{small:true}
			}
		},
		#h5: {
			options:{portrait:{if: =portrait, then:true}}
		},
		#h6: {
			options:{portrait:{if: =portrait, then:true}}
		},
		#h7: {
			options:{portrait:{if: =portrait, then:true}}
		},
		
		#line: {
			background-color:#b1aca1,
			frame: [50, 23, 262, 1]
		}
		
		
	}</style>
