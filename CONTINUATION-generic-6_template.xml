<jpml id="CONTINUATION-generic-6_template">
	<style src="CONTINUATION-stylesheet.ejs" />
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="PHONE-stylesheet.ejs" />
	<style>{
		$switch: [
			{if: =scale.phone, then: {
		        #b2: {
		                    height: {if:=articles.b2.subsection, equals:=articles.b1.subsection, then: 50} 
		        },
		        #b3: {
		                    height: {if:=articles.b3.subsection, equals:=articles.b2.subsection, then: 50} 
		        },
		        #b4: {
		                    height: {if:=articles.b4.subsection, equals:=articles.b3.subsection, then: 50} 
		        },
		        #b5: {
		                    height: {if:=articles.b5.subsection, equals:=articles.b4.subsection, then: 50} 
		        },
		        #b6: {
		                    height: {if:=articles.b6.subsection, equals:=articles.b5.subsection, then: 50} 
		        }
	        }},
	        {if: =portrait, then: {
	            #b1 : { frame: [0, 0, 256, 874] },
	            #b2 : { frame: [256, 0, 256, 874] },
	            #b3 : { frame: [512, 0, 256, 436] },
	            #b4 : { frame: [512, 0, 256, 291] },
				#b5 : { frame: [512, 291, 256, 291] },
				#b6 : { frame: [512, 582, 256, 291] }
	            
	        }},
			{if: =landscape, then: {
	            #b1 : { frame: [0, 0, 256, 618] },
	            #b2 : { frame: [256, 0, 256, 618] },        
	            #b3 : { frame: [512, 0, 256, 618] },
	            #b4 : { frame: [768, 0, 256, 204] },
				#b5 : { frame: [768, 204, 256, 204] },
				#b6 : { frame: [768, 410, 256, 204] }
	        }}
		]
    }</style>
    <box id="b1" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />    
    <box id="b2" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />
    <box id="b3" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />
    <box id="b4" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />
    <box id="b5" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />
    <box id="b6" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />
