<jpml id="SECTION-1story-P_template">
	<style src="GLOBAL-stylesheet.ejs" />
	<style src="SECTION-stylesheet.ejs" />
	<style>{
		if: =portrait, then: {
			#b1:	{ 
				frame: {$switch: [
					{if: =widescreen, then: [10, 0, 749, 620]},
					{if: =scale.large, then: [10, 0, 749, 620]}
				]},
				options:{P_image:true} 
			}
		}, else: {
			#b1:	{ 
						frame: {$switch: [
							{if: =widescreen, then: [18, 0, 749, 500]},
							{if: =scale.large, then: [18, 0, 749, 620]}
						]},
						options:{F_image:{if: =widescreen, then:true, else:false}, P_image:{if: =widescreen, then:false, else:true}} },
			#h2: {options:{hide_image:{if: =scale.small, then:true, else:false}}},
			#h1: {options:{hide_image:{if: =scale.small, then:true, else:false}}}
			
		}
	}</style>
	
	<box id="b1" embed="article" module="LARGE-hed-dek-image_module.xml" />
	<box id="h1" embed="article" module="HIGHLIGHTS_module.xml" />
	<box id="h2" embed="article" module="HIGHLIGHTS_module.xml" />
	<box id="ad_background" class="ad_light">
		<box id="highlight_ad" class="ad-rectangle" embed="advertisement" />
	</box>

	
</jpml>

