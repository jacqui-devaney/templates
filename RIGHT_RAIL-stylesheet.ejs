{
	jpml: {
		image-scale: 1,
		text-scale: 1,
		pixels-per-point-x: 1,
		pixels-per-point-y: 1,
		min-height: 0,
		padding:[0, 0, 0, 14]
	},
	.item: {
		font-size: 14,
		border-width:[0, 0, 1, 0],
		border-color:#d8d8d8,
		padding:[15, 14],
		height:275
	},
	.item-title: {
		font-name:DJ5EscrowCond-Semibold,
		font-size: 19,
		line-height:24,
		text-overflow: " \u00bb",
		color:#000,
		padding:[0, 0, 20, 0]
	},
	image: {
		pixels-per-point-x:1,
		pixels-per-point-y:1,
		width: {$subtract: [=view.width]},
		height: {$multiply: [=view.width, .66 ]}, 
		padding: [0, 0, 10, 0],
		image-scale: 1
	}
	
}
