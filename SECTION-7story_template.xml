<jpml id="SECTION-7story_template">
	<style src="GLOBAL-stylesheet.ejs" />
    <style src="GRID-stylesheet.ejs"/>
	<style src="ADVERTISING-stylesheet.ejs"/>
    <style src="PHONE-stylesheet.ejs"/>
	<style src="RAIL-stylesheet.ejs" />
	<style>{
		#page_details: {
			options: {
				tintTitle: true
			}
		}
	}</style>
    <style>{
    	#ad_placeholder: {
			display: inherit,
			frame: [0, 0, 10, 10]
		},
    	#marketstrap: {
    		display: {if: =page.section, equals: "TOP_NEWS", else: none},
    		position: relative
    	},
       #b1 : {
                height:{ if: =view.width, more-than: 320, then:{
                        if: {$count:[=articles.b1.images.phone]},
                        more-than: 0,
                        then: {$multiply: [=view.width, 1.139]},
                        else: {$multiply: [=view.width, 0.6]}
                        }, else: 418
                    },
					options: { bind: {if: =articles.b1.no_binding, then: none}, show_body: true, medium_headline: true }
				},

		#b2 : {
			height: =size.continuation_height_large,
			options: { bind: {if: =articles.b2.no_binding, then: none}, show_body: true }
		},
		#b3 : {
			height: =size.continuation_height_large,
			options: { bind: {if: =articles.b3.no_binding, then: none}, show_body: true }
		},
		#b4 : {
			options: {bind: {if: =articles.b4.no_binding, then: none}},
			height: =size.continuation_height_normal
		},
		#b5 : {
			options: {bind: {if: =articles.b5.no_binding, then: none}},
			height: =size.continuation_height_normal
		},
		#b6 : {
			options: {bind: {if: =articles.b6.no_binding, then: none}},
			height: =size.continuation_height_normal
		},
		#b7 : {
			options: {bind: {if: =articles.b7.no_binding, then: none}},
			height: =size.continuation_height_normal
		}
	}</style>
	<box id="page_details" embed="pagedescription" />
	<box id="ad_placeholder" class="ad-banner" embed="pagedescription" />
	<box id="marketstrap" embed="marketstrap" />
	<box id="b1" embed="article" class="column6-8 positionx1 maude unit_plain unit_418" module="TOP-IMAGE_module.xml" />
	<box id="b2" embed="article" class="maude unit_plain" module="CONTINUATION-dynamic_module.xml" />
	<box id="b3" embed="article" class="maude unit_plain" module="CONTINUATION-dynamic_module.xml" />
	<box id="b4" embed="article" class="maude unit_plain" module="CONTINUATION-dynamic_module.xml" />
	<box id="b5" embed="article" class="maude unit_plain" module="CONTINUATION-dynamic_module.xml" />
	<box id="b6" embed="article" class="maude unit_plain" module="CONTINUATION-dynamic_module.xml" />
	<box id="b7" embed="article" class="maude unit_plain" module="CONTINUATION-dynamic_module.xml" />
</jpml>
