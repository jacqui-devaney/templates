<jpml id="ARTICLE-front_template">
	<style src="GLOBAL-stylesheet.ejs"/>
  <style src="GRID-stylesheet.ejs"/>
  <style src="PHONE-stylesheet.ejs" />
  <style src="DELOITTE-stylesheet.ejs" />
  <style>{
    jpml: {
      background-color: =color.background,
      padding: {if: =scale.tablet, then: [64, 0, 0, 0]}
    },
    #page_details: {
      display: {if: =scale.phone, then: none},
      options: {
          hideHeaderBorder: true,
          tintTitle: true
      }
    },
    #deloitte_strap: {if: =scale.phone, then: {
      padding: [0, 12, 0, 12],
      display: {if: =page.number, not-equal-to: 0, then: none}
    }},
		#view: {if: =scale.phone, then: {
      height: {if: =page.number, equals: 0, then: {$multiply: [2, =view.height]} },
      bottom: {if: =page.number, not-equal-to: 0, then: 0}
    }, else: {
      top:0,
      bottom:24,
      padding: [40, 0, 0, 0]
    }}
  }</style>
  <style> {
		$switch: [
			{if: =scale.phone, then: {
        #view: {
          options: {
            background_color: =color.background,
            indent: false,
            module: {if: =page.number, equals: 0, then: url(ARTICLE-deloitte-front-phone_module.xml), else: url(ARTICLE-deloitte-continuation-phone_module.xml)}
          }
        }
			}},
			{
        #view: {
          options: {
            background_color: =color.background,
            indent: false,
            module: {if: =page.number, equals: 0, then: url(ARTICLE-deloitte-front_module.xml), else: url(ARTICLE-deloitte-continuation_module.xml)}
          }
        }
      }
		]
	}</style>
  <box id="page_details" embed="pagedescription" />
  <panel id="deloitte_strap">
    <box class="deloitte_strap_image"/>
    <p id="deloitte_strap_p"><mark id="deloitte_strap_mark"/></p>
  </panel>
  <box id="view" embed="article" class="column16-12 positionx1 maude" />
</jpml>
