{if: =scale.phone, then: {

	.key: {
		font-name: "DJ5Utility-Regular",
		font-size:16,
		content: "\u00a0K",
		display: { if-all: [=issue.locked, =article.paid], then: default, else: none },
		color:#948464
	},
	.module_summary: {
		font-name: DJ5Exchange-Medium,
		font-size: 16
	},
	.module_headline: {
		$join: [
			{
				padding: [0, 0, 8, 0],
				font-name: "DJ5Exchange-Semibold",
				color:{if: =article.read, then: #b8b8ba}
			},
			{
				$switch: [
					{if: =options.small_headline, then: {
						font-name: "DJ5Exchange-Semibold",
						font-size: 18,
						text-scale: {$multiply: [=scale.text, 1] },
						line-height: 25
					}},
					{
						font-size: 26,
						line-height: 36
					}
				]
			}
		]
	},
	.body: {
		text-indent:0,
		font-size:16,
		line-height:24,
		text-scale: {$multiply: [=scale.text, 1] },
		color:{if: =article.read, then: #b8b8ba}

	},
	.ahed_headline:{
		font-size:20,
		line-height:20,
		text-scale: {$multiply: [=scale.text, 1] },
		padding:[0, 0, 0, 0],
		text-overflow: =text.overflow,
		font-name:DJ5EscrowCond-Bold,
		color:#333

	},
	.highlight_flashline: {
		font-name:DJ5RetinaCd-Semibold,
		font-size:16,
		line-height:18,
		color:#656565,
		paragraph-padding: [ 0, 0, 20, 0 ]
	},

	#deco_headline: {
		text-scale: {$multiply: [=scale.text, 1] },
		font-size:18,
		line-height:18
	},
	#deco_bottom_headline: {
		font-size:21,
		line-height:21
	},
	.deco_caption: {
		font-size:14,
		line-height:14
	},
	#strap: {
		display: { if:=options.show_strap, then:default, else:none },
		padding: [8.5, 8, 8.5, 8],
		border-width: [0.5, 0, 0.5, 0],
		border-color: black
    },
	.subsection: {
		font-name: "DJ5Exchange-Bold",
		font-size: 17,
		line-height: 16,
		color: =page.tint,
		text-transform: uppercase
	},
	.strap: {
		font-size:15,
		text-scale: {$multiply: [=scale.text, 1] },
		line-height:16,
		text-transform:uppercase
	},
	.highlight_flashline: {
		font-size:14,
		padding: [ 0, 0, 0, 0 ],
		line-height:14
	},
	.highlight_headline: {
		font-size:16,
		text-scale: {$multiply: [=scale.text, 1] },
		line-height:16,
		padding:[0],
		text-overflow: =text.overflow
	},
	.highlight_large_headline: {
		font-size:16,
		text-scale: {$multiply: [=scale.text, 1] },
		line-height:16,
		padding:[0],
		text-overflow: =text.overflow
	},
	.highlight_flashline_small: {
		font-size:14,
		line-height:14
	},
	.article_flashline: {
		font-size:15,
		text-align: center,
		font-name: "DJ5Exchange-Bold",
		text-transform:uppercase,
		padding: [10,0,0,0]
	},
	.article_headline: {
		font-size:23,
		line-height:26,
		font-name: "DJ5Exchange-SemiBold",
		padding: [10,0,10,0]
	},
	.article_subhed: {
		font-size:17,
		line-height:20,
		font-name: "DJ5Exchange-BoldItalic",
		color: #5D5D5D
	},
	.article_byline: {
		font-name: "DJ5Exchange-Bold",
		font-size:15,
		line-height:20
	},
	.article_pubdate: {
		font-name: "DJ5Exchange-Medium",
		font-size:15,
		line-height: 15
	},
	.article_body: {
		font-size:{$multiply: [16, =view.zoomlevel]},
		line-height:{$multiply: [20, =view.zoomlevel]},
		font-name:"DJ5Exchange-Medium",
		text-indent: 13
	},
	.border-mark: {
		line-height: 4,
		color: transparent,
		content: "unfortunate"
	},
	.border-p:{
		border-width:[10, 180, 10, 0],
		background-color: #e1e0df,
		border-color:white
	},
	.border-flashline-mark: {
		line-height: 1,
		color: transparent,
		content: "unfortunate"
	},
	.border-flashline:{
		border-width:[0, 0, 0, 0],
		background-color: #e1e0df,
		border-color:white
	},
	#media: {
		display: {if: =page.hasmedia, then:inherit, else:none}
	},
	#pagenext: {
		display: none
	},
	#pageback: {
		display: none
	},
	#pagedown: {
		right: 0,
		bottom: 0,
		width: 320,
		height:150,
		position:absolute,
		display:{ if: =app.version, at-least: 5.4, then: none, else: inherit }
	},
	#pageup: {
		top: 0,
		left: 0,
		height:150,
		width: 320,
		position:absolute,
		display:{ if: =app.version, at-least: 5.4, then: none, else: inherit }

	},

	.opinion_headline: {
		font-size:18,
		text-scale: {$multiply: [=scale.text, 1] },
		line-height:25,
		font-name:DJ5EscrowCond-Bold,
		text-align:left
	},
	.opinion_dek: {
		font-size:14,
		line-height:17,
		text-align:left,
		text-scale: {$multiply: [=scale.text, 1] },
		text-overflow: =text.overflow
	},
	.cio_head: {
		color:#000,
		font-name:DJ5RetinaCd-Medium,
		text-transform:uppercase,
		text-align:left,
		font-size:18,
		text-scale: {$multiply: [=scale.text, 1] }
	},
	.tech_section: {
		font-size:22,
		line-height:23
	},
	.tagline: {
		font-size:11
	},
	.cio_highlight_headline: {
		text-overflow: =text.overflow
	},
	#extra_small_headline: {
		font-size:20,
		line-height:20,
		padding:[2, 0, 0, 0],
		text-scale: {$multiply: [=scale.text, 1] }
	},

	.large_dek: {
		font-name:DJ5Exchange-Medium,
		text-indent:0,
		font-size:18,
		line-height:20,
		text-overflow: =text.overflow,
        text-align: left,
		text-scale: {$multiply: [=scale.text, 1] },
		color:#000
	},
	.vertical_center: {
		vertical-align:middle
	},
	.hidden_tablet: {
		display: inherit
	},
	.hidden_phone: {
		display: none
	},

	#ad_inline_inner: {
		pixels-per-point-x: 1,
		pixels-per-point-y: 1,
		anchor-x: center,
		anchor-y: middle,
		options: { zone: {$join: [=ad.prefix, abanner]} },
		width: 320,
		height: 50,
		background-color: #e7e2d8
	},

	#ad_inline_outer: {
		pixels-per-point-x: 1,
		pixels-per-point-y: 1,
		position: relative,
		min-height: 100,
		height: 50,
		left: -12,
		top: -80,
		right: -12,
		padding: [10, 0],
		display: none
	},

	#ad_inline_panel: {
		pixels-per-point-x: 1,
		pixels-per-point-y: 1,
		padding: [10, 0],
		min-height: 50,
		display: none
	},

	image: {
		background-color:#e4e4e4
	},
	.ticker: {
		display: {if: =intent.article, then: inherit, else: none},
		color: transparent
	}

}, else:{}}
