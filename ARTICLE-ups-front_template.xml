<jpml id="ARTICLE-ups-front_template">
	<style src="GLOBAL-stylesheet.ejs"/>
    <style src="GRID-stylesheet.ejs"/>
    <style src="UPS-stylesheet.ejs" />
    <style>{
        #page_details: {
            options: {
                hideHeaderBorder: true,
                tintTitle: true
            }
        }
    }</style>
	<style>{
		#view: {
            top:0,
            bottom:44
        }
    }</style>
	<style>{
		#view: {
			options: {
				background_color: =color.background,
				indent: true
			},
            padding: [40, 0, 0, 0]
		},
		jpml: {
			background-color: =color.background,
			padding: [64, 0, 0, 0]
		}
	}</style>
    <box id="page_details" embed="pagedescription" />
    <panel id="ups_strap">
        <box class="ups_strap_image"/>
        <p id="ups_strap_p"><mark id="ups_strap_mark"/></p>
    </panel>
    <box id="view" embed="article" class="column16-12 positionx1" module="ARTICLE-ups-front_module.xml" />
</jpml>
