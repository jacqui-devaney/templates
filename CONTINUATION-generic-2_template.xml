<jpml id="CONTINUATION-generic-2_template">
	<style src="CONTINUATION-stylesheet.ejs" />
    <style src="GLOBAL-stylesheet.ejs" />
    <style src="PHONE-stylesheet.ejs" />
	<style>{
		#b2: {options: {hide_strap: {if:=articles.b2.subsection, equals:=articles.b1.subsection}}}
	}</style>
	<style>{
		$switch: [
	        {if: =scale.phone, then: {
		        #b2: {
		                    height: {if:=articles.b2.subsection, equals:=articles.b1.subsection, then: 44} 
		        }
	        }},
	        {if: =portrait, then: {
	            #b1 : { frame: [0, 0, 256, 874] },
	            #b2 : { frame: [256, 0, 256, 874] }
	        }}, 
			{if: =landmark, then: {
	            #b1 : { frame: [0, 0, 256, 618] },
	            #b2 : { frame: [256, 0, 256, 618] }        
	        }}
		]
    }</style>
    <box id="b1" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />    
    <box id="b2" embed="article" class="maude unit_continuation_strap" module="CONTINUATION-dynamic_module.xml" />
</jpml>