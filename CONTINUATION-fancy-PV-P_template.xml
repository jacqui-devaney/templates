<jpml id="CONTINUATION-fancy-PV-P_template">
	<style src="CONTINUATION-stylesheet.ejs" /> 
	<style src="GLOBAL-stylesheet.ejs" /> 
	<style>{
        if: =portrait, then: {
			#deco : { 
				frame: {$switch: [
					{if: =widescreen, then: [0, 0, 768, 620]},
					{if: =scale.large, then: [0, 0, 768, 620]}
				]},
				options: {bottom_headline:true, bottom_border:true} 
			},
            #b1 : { 
					frame: {$switch: [
						{if: =widescreen, then: [0, 630, 256, 250]},
						{if: =scale.large, then: [0, 630, 256, 250]}
					]},
					options: {hide_strap: true}
			},
            #b2 : { 
					frame: {$switch: [
						{if: =widescreen, then: [260, 630, 256, 250]},
						{if: =scale.large, then: [260, 630, 256, 250]}
					]},
					options: {hide_strap: true} 
			},
            #b3 : { 
					frame: {$switch: [
						{if: =widescreen, then: [520, 630, 256, 250]},
						{if: =scale.large, then: [520, 630, 256, 250]}
					]},
					options: {hide_strap: true} 
			}

        }, else: {
	        #b1 : { frame: [770, 0, 256, 208], options: {hide_strap: true} },
	        #b2 : { frame: [770, 205, 256, 208], options: {hide_strap: true} },
	        #b3 : { frame: [770, 413, 256, 208], options: {hide_strap: true} },
	        #deco : { frame: [0, 0, 768, 620], options: {bottom_headline:true, bottom_border:true, F_image:{if: =widescreen, then:true, else:false}} }
        }
    }</style>
    <box id="b1" embed="article" module="CONTINUATION-dynamic_module.xml" />    
    <box id="b2" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="b3" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="b4" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="b5" embed="article" module="CONTINUATION-dynamic_module.xml" />
    <box id="deco" embed="article" module="DECO_module.xml" />

</jpml>

