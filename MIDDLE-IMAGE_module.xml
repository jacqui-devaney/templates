<jpml id="MIDDLE-IMAGE_module">
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="GLOBAL-stylesheet.ejs" />
	<style>{
		#body_container: { padding: [10] },
        #body: { 
            column-count: {$divide: [=view.width, {$multiply:[250, =scale.dpi]}]},
            column-gutter: 20
        },
        #image_container: { 
			width:359, 
			anchor-y:top,
			anchor-x:left
			
			 }
		}</style>
	<panel id="not_prominent">
		<p class="small_headline"><span embed="headline" /><mark class="key" /></p>
	</panel>
	<panel id="medium">
		<p class="medium_headline"><span embed="headline" /><mark class="key" /></p>
	</panel>
	<panel id="prominent">
		<p class="large_headline"><span embed="headline" /><mark class="key" /></p>
	</panel>
	<box id="image_container">
		<image embed="summary-image"/>
	</box>
	<panel id="body_container">
        <panel id="body">
            <p class="byline" embed="byline"/>
            <panel class="body" embed="body"/>
        </panel>
    </panel>

</jpml>
