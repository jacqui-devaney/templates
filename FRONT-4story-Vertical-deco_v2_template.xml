	<style> {
		jpml: {border-width:[1, 0, 0, 0], border-color:black},
		#b1: {options: {$switch: [
			{if: =scale.phone, then: {large_headline:true, no_border:true, show_body:true, top_align:true}},
			{if: =portrait, then: {
					medium_headline:true, 
					no_border:true,
					byline:{if: =scale.small, then:true, else:false}
				}},
			{if: =landscape, then: {medium_headline:true, no_border:true, byline:{if: =scale.small, then:true, else:false}}}		
		]}},
		#b2: {options: {$switch: [
			{if: =scale.phone, then: {small_headline:true}},
