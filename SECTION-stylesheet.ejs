{
	#h1: {
		top: {if: =landscape, then:0, else:706},
		height: {$switch:[
			{if-all:[{if: =landscape}, {if: =page.number, equals:0, then:true}], then:235},
			{if: =portrait, then: 256},
			{if-all:[{if: =landscape}, {if: =page.number, equals:1, then:true}], then:353}
		]},
		width:{$switch:[
			{if-all:[=portrait, {if: =page.number, equals:1}], then:374},
			{if: =landscape, then:251},
			"246"
		]}
	},
	#h2: {
		top: {$switch:[
			{if-all:[{if: =landscape}, {if: =page.number, equals:0, then:true}], then:235},
			{if: =portrait, then: 706},
			{if-all:[{if: =landscape}, {if: =page.number, equals:1, then:true}], then:353}
		]},

		height: {$switch:[
			{if-all:[{if: =landscape}, {if: =page.number, equals:0, then:true}], then:233},
			{if: =portrait, then: 256},
			{if-all:[{if: =landscape}, {if: =page.number, equals:1, then:true}], then:353}
		]},
		width:{$switch:[
			{if-all:[=portrait, {if: =page.number, equals:1}], then:374},
			{if: =landscape, then:251},
			"246"
		]},
		left:{$switch:[
			{if-all:[=portrait, {if: =page.number, equals:0}], then:261},
			{if-all:[=portrait, {if: =page.number, equals:1}], then:389},
			"773"
		]}
	},
	#h3: {
		top:{if: =landscape, then:0, else:706},
		height: {if: =landscape, then:471, else:256},
		width:{if: =landscape, then:251, else:517}
	},
	#background_color: {
		background-color:{if: =options.white_highlights, then:#fff, else:#efefef}, 
		width:{if: =landscape, then:251, else:768},
		height:{if: =landscape, then:706, else:256},
		left:{if: =landscape, then:771, else:0},
		top:{if: =landscape, then:0, else:706}
	},
	#marketdata_background: 	{ 
		frame: [0, 620, 256, 257],
		border-width: [ 0, 1, 0, 0],
		border-color:#b1aca1
	},
	#highlight_marketdata: 	{ 
			anchor-x:center,
			anchor-y:middle,
			width:239,
			height:239
	}
	
}
