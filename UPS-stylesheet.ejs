{
	strong: {
		font-name:{if: =intent.article, then:Whitney-Semibold}
	},
	em: {
		font-name:{if: =intent.article, then:Whitney-MediumItalic}
	},
	cite: {
		font-name:{if: =intent.article, then:Whitney-BookItalic}
	},
	#sponsored_strap:{
		min-height: 40,
		max-height: 40,
		background-color: #E7EDF0,
		padding: [0, 18, 0, 18]
	},
	#sponsored_strap_p:{
		text-align: right,
		padding: [14, 0, 0, 0]
	},
	#sponsored_strap_mark:{
		font-name: "Whitney-Semibold",
		font-size: 12,
		color: #333333,
		content: "CONTENT FROM OUR SPONSOR"
	},
	.sponsored_strap_image:{
        background-image: "url(UPS-module@2x.png)",
        position: relative,
		background-position: aspect-fit,
		top: 10,
		left: 0,
		width: 87,
		height: 87
	},

	#sponsored_list: {
		min-height: 0,
		max-height: 10000,
		background-color: #e6e5df
	},

	#sponsored_inner: {
		padding: [0, 15]
	},

	#sponsored_phone: {
		background-color: #e6e5df,
		padding: [0, 10]
	},

	.sponsored_item: {
		padding: { if: =scale.phone, then: [15, 0, 14.5, 0], else: [20, 0, 19.5, 0] },
		border-width: [0, 0, 0.5, 0],
		border-color: #e3e4e3
	},

	.sponsored_item_first: {
		if: =scale.phone, then: {
			padding: [14.5, 0, 14.5, 0],
			border-width: [0.5, 0, 0.5, 0],
			border-color: #e3e4e3
		}, else: {
			padding: [14.5, 0, 19.5, 0],
			border-width: [0.5, 0, 0.5, 0],
			border-color: #e3e4e3
		}
	},

	.sponsored_item_last: {
		padding: { if: =scale.phone, then: [15, 0, 15, 0], else: [20, 0, 20, 0] },
		border-width: [0]
	},

	.sponsored_headline: {
		font-name: "Whitney-Medium",
		font-size: 20,
		line-height: 25,
		color: #351c15,
		padding: [0, 0, 5, 0]
	},
	.sponsored_summary: {
		font-name: "Whitney-Book",
		font-size: { if: =scale.phone, then: 16, else: 14 },
		line-height: { if: =scale.phone, then: 25, else: 18 },
		color: #333333
	},

	.body-last-p-mark: {
			display: none
	},

	.article_body: {
		text-align:left,
		text-indent:0
	},

	h2: {
		text-indent:0
	},

	h3: {
		text-indent: 0,
		text-align: center
	},
	
	p: {
		padding:[0,0,20,0]
	},

	#body-first-p: {
		padding:[0,0,20,0]
	},

	#body-last-p: {
		padding: [0, 0, 8, 0]
	},

	.sponsoredFOOTNOTES: {
		padding: [13, 0, 10, 0]
	},

	.sponsoredRELATEDCONTENT: {
		padding: [12, 0, 0, 0]
	},

	.sponsoredQUESTIONS: {
		padding: [10, 0, 8, 0]
	},

	.sponsoredDISCLAMER: {
		padding: [13, 0, 0, 0]
	},

	.sponsored-divider-p: {
		padding: [0, 0, 0, 0],
		border-width: [1, 0, 0, 0],
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		background-color: transparent,
		border-color: #FFFFFF
	},

	.sponsored-divider-mark: {
		font-size: 1,
		line-height: 1,
		color: transparent,
		content: "divider"
	},

	.sponsored-footnotes: {
		font-name: "Whitney-Medium",
		font-size: { if: =scale.phone, then: {$multiply: [14, =view.zoomlevel]}, else: {$multiply: [11, =view.zoomlevel]} },
		line-height: { if: =scale.phone, then: {$multiply: [18, =view.zoomlevel]}, else: {$multiply: [15, =view.zoomlevel]} },
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		color: #898B8E
	},

	.sponsored-related-content: {
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		font-name: "Whitney-Medium",
		color: #222222
	},

	.sponsored-questions: {
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		font-name: "Whitney-Medium",
		color: #222222
	},

	.sponsored-disclaimer: {
		font-name: "Whitney-Medium",
		font-size: { if: =scale.phone, then: {$multiply: [14, =view.zoomlevel]}, else: {$multiply: [11, =view.zoomlevel]} },
		line-height: { if: =scale.phone, then: {$multiply: [18, =view.zoomlevel]}, else: {$multiply: [15, =view.zoomlevel]} },
		layout-grid-line: 0,
		layout-grid-line-offset: 0,
		color: #222222
	},

	#sponsored_item_1: { if: true, then: { options: { reference: =articles.d1.link } } },
	#sponsored_item_2: { if: true, then: { options: { reference: =articles.d2.link } } },
	#sponsored_item_3: { if: true, then: { options: { reference: =articles.d3.link } } },
	#sponsored_item_4: { if: true, then: { options: { reference: =articles.d4.link } } },
	#sponsored_item_5: { if: true, then: { options: { reference: =articles.d5.link } } },
	#sponsored_item_6: { if: true, then: { options: { reference: =articles.d6.link } } },

	#sponsored_headline_1: { content: =articles.d1.headline },
	#sponsored_headline_2: { content: =articles.d2.headline },
	#sponsored_headline_3: { content: =articles.d3.headline },
	#sponsored_headline_4: { content: =articles.d4.headline },
	#sponsored_headline_5: { content: =articles.d5.headline },
	#sponsored_headline_6: { content: =articles.d6.headline },

	#sponsored_summary_1: { content: { $switch: [=articles.d1.websummary, =articles.d1.summary] } },
	#sponsored_summary_2: { content: { $switch: [=articles.d2.websummary, =articles.d2.summary] } },
	#sponsored_summary_3: { content: { $switch: [=articles.d3.websummary, =articles.d3.summary] } },
	#sponsored_summary_4: { content: { $switch: [=articles.d4.websummary, =articles.d4.summary] } },
	#sponsored_summary_5: { content: { $switch: [=articles.d5.websummary, =articles.d5.summary] } },
	#sponsored_summary_6: { content: { $switch: [=articles.d6.websummary, =articles.d6.summary] } }

}
