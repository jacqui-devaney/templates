<pages preprocessor="preprocessor.ejs" class="article">

    <!-- This template will always be used to render the front page -->
	<page template="ARTICLE-front-deloitte_template.xml" type="prefix" for="tablet">
		<link id="view" module="ARTICLE-front-deloitte_module.xml"/>
	</page>
	
    <!-- These pages will repeat as needed, hence the "repeat" type -->
	<page template="ARTICLE-continuation-deloitte-no-ads_template.xml" type="repeat" for="tablet">
		<link id="view" module="ARTICLE-continuation-deloitte-no-ads_module.xml"/>
	</page>	
	
    <!-- The last page of every article uses column leveling with a background matte,
     which is not supported in conjunction with the ads template. The "suffix" type
     is used to render that last page. -->
	<page template="ARTICLE-continuation-deloitte-no-ads_template.xml" type="suffix" for="tablet">
		<link id="view" module="ARTICLE-continuation-deloitte-no-ads_module.xml"/>
	</page>	
	
	<page template="ARTICLE-front-deloitte-phone_template.xml" type="prefix" for="phone">
		<link id="view" module="ARTICLE-front-deloitte-phone_module.xml"/>
	</page>
	<page template="ARTICLE-continuation-deloitte-phone-no-ads_template.xml" type="repeat" for="phone">
		<link id="view" module="ARTICLE-continuation-deloitte-phone-no-ads_module.xml"/>
	</page>	
	<page template="ARTICLE-continuation-deloitte-phone-no-ads_template.xml" type="suffix" for="phone">
		<link id="view" module="ARTICLE-continuation-deloitte-phone-no-ads_module.xml"/>
	</page>	
	
	
	
</pages>
