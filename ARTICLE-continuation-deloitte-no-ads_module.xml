<jpml id="ARTICLE-continuation-deloitte-no-ads_module">
	<style src="GLOBAL_MODULES-stylesheet.ejs" />
	<style src="GLOBAL-stylesheet.ejs" />
	<style>{
		$join: [
		{#deloitte_article_header: {
			frame: [0, 0, 1024, 53]
		}},
		{.deloitte:{
			background-position: top-left
		}},
		{.deloitte_article:{
			padding:[6, 0, 0, 15]
		}},
		{.deloitte_slug:{
			text-align:left,
			padding:[0, 0, 0, 15]
		}},
		{.close_button: {
			background-image:url(close.png),
			background-position:top-right,
			width:60,
			height:30
		}},
		{#body_continuation: {
			content: from-flow(article_body),
			flow: article_body
		}},
		{.body-bug: {
			display: none
		}},

		{#body_container: { 
			padding: [10, 20, 0, 20],
			column-count: {$divide: [=view.width, {$multiply:[260, =scale.dpi]}]},
            column-gutter: 30,
			column-height:balance,
			layout-grid-line: {$multiply: [22, =view.zoomlevel]},
			layout-grid-line-offset: {$multiply: [18, =view.zoomlevel]},
			border-width:[1, 0, 0, 0],
			border-color:#e7e7e7,
			max-height:{if: =portrait, then: 680, else:430}
        }},
		{#deloitte_legal:{
			padding:[0, 0, 20, 20],
			anchor-y:bottom,
			anchor-x:left,
			width:647
		}}

		]
    }</style>
	<box id="deloitte_article_header">
		<box id="close_button_box">
			<box class="close_button" embed="close"/>
		</box>
		<panel class="deloitte_article">
			<panel class="deloitte" />
		</panel>
		<panel class="slug">
			<p class="deloitte_slug" >Business-led, Technology-enabled: Insight from Deloitte</p>
		</panel>
	</box>
	<panel id="body_container">
        <box id="continued_label">
			<p><mark id="continued" /></p>
		</box>
		<panel class="deloitte_article_body" id="body_continuation" embed="body"/>
	    
	</panel>
</jpml>


