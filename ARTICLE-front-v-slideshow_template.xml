<jpml id="ARTICLE-front-v-slideshow_template">
    <style src="GLOBAL-stylesheet.ejs"/>
    <style src="GRID-stylesheet.ejs"/>
    <style>{
        #page_details: {
            options: {
                tintTitle: true,
                tintColor: { if: =page.section, equals: "WSJ_MAGAZINE", then: #66, else: white },
            }
        }
    }</style>
    <style>{
        #view: {
            top: 0,
            bottom: 40,
            options: {
                indent: false
            }
        }
    }</style>
    <box id="page_details" embed="pagedescription" />
    <box id="view" embed="article" class="landscape-columns-16 portrait-columns-12 xposition-1" module="ARTICLE-front-v-slideshow_module.xml" />
</jpml>
